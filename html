					openId = $("button[name=ibtnOpen], div[name=ibtnOpen]").attr("id");

					$("#"+openId).attr("name", "ibtnClose");
					$("#"+openId).removeClass("open");
					deleteLayer();
				});

				var idx = jQuery(".dep1Tab").index(this);

				jQuery(".dep1Tab").removeClass("active");
				jQuery(".mTab").hide();

				jQuery(this).addClass("active");
				jQuery(".mTab").eq(idx).show();
			});

			jQuery(".dep2Tab").on("click focus",function(){
				if(jQuery(this).parent().find("ul").size() !=0){
					var idx = jQuery(".dep2Tab").index(this);
					jQuery(".dep2Tab").removeClass("active");
					jQuery(".dep3TabBox").hide();

					jQuery(this).addClass("active");

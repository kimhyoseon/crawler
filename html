		//title , h2 , location 을 location 기준으로 정리하여 변경 처리함.
		function fn_cngTitle(){
			//console.log("JSP PAGE&gt;&gt;" + jsCurrentJsp);
			//alert($('.trailMenu').html());
			var lctTxt = $('.trailMenu');
			var titleTxt = "";
			lctTxt.find("span").eq(0).text("JEJUAIR"); //HOME 에서 -&gt; JEJUAIR 로 변경
			if(lctTxt.find("span").length != 0){
				for(var i = lctTxt.find("span").length - 1 ; i &gt;= 0; i--){
					if(i == 0){
						h2Txt = lctTxt.find("span").eq(i).text();
						titleTxt = titleTxt + lctTxt.find("span").eq(i).text();
					}else{
						titleTxt = titleTxt + lctTxt.find("span").eq(i).text() +"&lt;";
					}
				}

				if(!$('.title0').hasClass("noT")){ //게시판 부분은 예외 처리 KANG
					$('.title0').text(lctTxt.find("span").eq(lctTxt.find("span").length - 1).text() );
				}
				// SEO 테스트 추가(20170419)
				titleTxt = "메인 | 제주항공" + titleTxt;

				$(document).prop('title', titleTxt);
			}else if(lctTxt.find("a").length != 0){
				for(var i = lctTxt.find("a").length - 1 ; i &gt;= 0; i--){
					if(i == 0){
						h2Txt = lctTxt.find("a").eq(i).text();
						titleTxt = titleTxt + lctTxt.find("a").eq(i).text();
					}else{
						titleTxt = titleTxt + lctTxt.find("a").eq(i).text() +"&lt;";
					}
				}
				if(!$('.title0').hasClass("noT")){
					$('.title0').text(lctTxt.find("a").eq(lctTxt.find("a").length - 1).text() );
				}

				// SEO 테스트 추가(20170419)
				titleTxt = "메인 | 제주항공" + titleTxt;

				$(document).prop('title', titleTxt);
			}else{
				// SEO 테스트 추가(20170419)
				titleTxt = "" + titleTxt;

				//$(document).prop('title', "JEJUAIR");
				$(document).prop('title', titleTxt);
			}

		}


		//웹접근성(Tocanz) - 20151103 - Kang
		function fn_topContentsEng(){
			//영문타입 변경
			$('html').attr("lang" , "en");
			$('html').attr("xml:lang" , "en");

			//주메뉴 바로가기 변경
			var naviDiv = $('#JEJUAIR_SKIP_NAVI_DIV');
			naviDiv.find("span").eq(0).text("Go to the main context");
			naviDiv.find("span").eq(1).text("Go to the main menu");
		}



		//--------------------------------------
		// 사용자가 상단에서 로그인 했을 경우 로그인 정보를 보여주기 위함.
		// 2015.09.20 FFP시스템개선
		//--------------------------------------
		function fn_setSessionTop(){

			$.post(jsContextRoot + "/com/jeju/ibe/login/join/chkLogin.do", function(data) {

				var thisUrl =   location.href;
				if ( thisUrl.indexOf("companyPreference/main.do") &gt; -1){	// 기업우대서비스 페이지일 경우 새로 고침
					window.location.reload();
				}


				try {
					jsTopIsMember = data.SS_MBR_ACT;
					jsTopLoginCheck = "true";
					jsTopLoginUserID = data.SS_IBE_USERID;
					jsTopLoginUserName = data.SS_MBR_NAME;
					jsTopLoginUserNameL = data.SS_MBR_KLNAME;
					jsTopLoginUserNameF = data.SS_MBR_KFNAME;
					jsTopLoginEngUserNameL = data.SS_MBR_ELNAME;
					jsTopLoginEngUserNameF = data.SS_MBR_EFNAME;
					jsTopLoginFFPNo = data.SS_MBR_JJNO;
					jsTopLoginFFPGrade = data.SS_FFP_GRADE;
					jsTopFFPGradeFromDate = data.SS_FFP_GRADE_FR;
					jsTopFFPGradeToDate = data.SS_FFP_GRADE_TO;
					jsTopFFPNextGradePoint = data.SS_FFP_NEXT_GRADE_POINT;
					jsTopFFPNextGradeBoardCnt = data.SS_FFP_NEXT_GRADE_BOARDCNT;

					if (jsTopLoginFFPGrade == "S") {
						jsTopLoginFFPGradeName = "SILVER";
					} else if (jsTopLoginFFPGrade == "G") {
						jsTopLoginFFPGradeName = "GOLD";
					} else if (jsTopLoginFFPGrade == "V") {
						jsTopLoginFFPGradeName = "VIP";
					}
					if (jsTopLoginUserNameL == "") jsTopLoginUserNameL = jsTopLoginEngUserNameL;
					if (jsTopLoginUserNameF == "") jsTopLoginUserNameF = jsTopLoginEngUserNameF;
					if (jsTopLoginUserName == "") jsTopLoginUserName = jsTopLoginEngUserNameL + jsTopLoginEngUserNameF;

					// 언어에 따른 이름 적용
					if (jsTopLang != "KR") jsTopLoginUserName = jsTopLoginEngUserNameL + jsTopLoginEngUserNameF;

					if (jsTopLoginFFPNo != undefined &amp;&amp; jsTopLoginFFPNo != ""){

						// 보유포인트 조회
						jsTopFFPPointObj = $.cfn_searchFFPPoint("CQ", jsTopLoginFFPNo, "JPNT");

						// 로그인 영역의 회원정보 셋팅
						//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name") + " " + $.cfn_currencyExp(jsFFPPointObj.DATA.BALANCEPOINT, "P", true, true));
						$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
						$("#gnb_refresh_index").html($.cfn_currencyExp(jsTopFFPPointObj.DATA.BALANCEPOINT, "P", true, true));

						// 각 화면의 회원정보 및 버튼 셋팅
						fn_controlCommonUserInfo("TOP");

					} else {
						jsTopLoginUserName = jsTopLoginUserID;

						// 로그인 영역의 회원정보 셋팅
						//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name"));
						$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
					}

					// Top 로그인 정보를 로컬의 로그인 정보에 셋팅한다.
					fn_setLoginInfoToLocal();
				} catch (e) {
					log ("***** fn_setSessionTop EXCEPTION *****");
					log ("Error Message: " + e.message);
					log ("Error Code: " + e.number);
					log ("Error Name: " + e.name);
				} finally {
					// 각 화면에 처리해야할 로직이 있을 경우 함수 호출
					if ( eval("typeof fn_setSession") == "function" ) fn_setSession();
				}


			});
		}

		function htmlspecialchars(string, quote_style, charset, double_encode) {
			var optTemp = 0,
				i = 0,
				noquotes = false;
			if (typeof quote_style === 'undefined' || quote_style === null) {
				quote_style = 2;
			}
			string = string.toString();
			if (double_encode !== false) { // Put this first to avoid double-encoding
				string = string.replace(/&amp;/g, '&amp;amp;');
			}

			//&lt;![CDATA[
			// 접근성개선 : BLUEGRID
			string = string.replace(/&lt;/g, '&amp;lt;').replace(/&gt;/g, '&amp;gt;');
			//]]&gt;

			var OPTS = {
				'ENT_NOQUOTES': 0,
				'ENT_HTML_QUOTE_SINGLE': 1,
				'ENT_HTML_QUOTE_DOUBLE': 2,
				'ENT_COMPAT': 2,
				'ENT_QUOTES': 3,
				'ENT_IGNORE': 4
			};
			if (quote_style === 0) {
				noquotes = true;
			}
			if (typeof quote_style !== 'number') { // Allow for a single string or an array of string flags
				quote_style = [].concat(quote_style);
				for (i = 0; i &lt; quote_style.length; i++) {
					// Resolve string input to bitwise e.g. 'ENT_IGNORE' becomes 4
					if (OPTS[quote_style[i]] === 0) {
						noquotes = true;
					} else if (OPTS[quote_style[i]]) {
						optTemp = optTemp | OPTS[quote_style[i]];
					}
				}
				quote_style = optTemp;
			}
			if (quote_style &amp; OPTS.ENT_HTML_QUOTE_SINGLE) {
				string = string.replace(/'/g, '&amp;#039;');
			}
			if (!noquotes) {
				string = string.replace(/"/g, '&amp;quot;');
			}
			return string;
		}

		// 회원의 보유포인트 조회. 2015.09.20 FFP시스템개선
		function fn_searchFFPInfo(loginFlag) {
			try {
				if (loginFlag == "Login") {

					if (typeof jsFFPPointObj == "undefined" || typeof jsFFPPointObj.DATA == "undefined") {
						jsTopFFPPointObj = $.cfn_searchFFPPoint("CQ", jsTopLoginFFPNo, "JPNT");

						//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name") + " " + $.cfn_currencyExp(jsTopFFPPointObj.DATA.BALANCEPOINT, "P", true, true));
						$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
						$("#gnb_refresh_index").html($.cfn_currencyExp(jsTopFFPPointObj.DATA.BALANCEPOINT, "P", true, true));
					} else {
						//jsFFPPointObj = $.cfn_searchFFPPoint("CQ", jsTopLoginFFPNo, "JPNT");

						//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name") + " " + $.cfn_currencyExp(jsFFPPointObj.DATA.BALANCEPOINT, "P", true, true));
						$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
						$("#gnb_refresh_index").html($.cfn_currencyExp(jsFFPPointObj.DATA.BALANCEPOINT, "P", true, true));
					}

				} else if (loginFlag == "noLogin") {
					//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name"));
					$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
				}
			} catch (e) {
				log ("***** fn_searchFFPInfo EXCEPTION *****");
				log ("Error Message: " + e.message);
				log ("Error Code: " + e.number);
				log ("Error Name: " + e.name);
			}
		}

		// 회원 정보 및 버튼 컨트롤. 2015.10.07 FFP시스템개선
		function fn_controlCommonUserInfo(type) {
			var jsTmpFFPPointObj = {};

			try {
				if (type == "TOP") {
					jsTmpFFPPointObj = jsTopFFPPointObj;
				} else {
					jsTmpFFPPointObj = jsFFPPointObj;
				}

				jsTopLoginFFPPoint = jsTmpFFPPointObj.DATA.BALANCEPOINT;

				$("#divUserName").html($.cfn_getMessageByScreenWithParam(jsonData, "hiUserName", [jsTopLoginUserName]));
				$("#pFFPNo").html($.cfn_getMessageByScreenWithParam(jsonData, "spFFPNo", [jsTopLoginFFPNo]));
				$("#liFFPGrade").html(jsTopLoginFFPGradeName);

				// 등급에 따른 색상 적용
				if (jsTopLoginFFPGrade == "G") {
					$("#liFFPGrade").addClass("gold");
				} else if (jsTopLoginFFPGrade == "V") {
					$("#liFFPGrade").addClass("vip");
				}

				$("#liFFPPoint").html($.cfn_currencyExp(jsTmpFFPPointObj.DATA.BALANCEPOINT, "P", true, true));		// 총누적
				$("#ddBalance").html($.cfn_currencyExp(jsTmpFFPPointObj.DATA.BALANCEPOINT, "P", true, true));	// 가용
				$("#ddExint").html($.cfn_currencyExp(jsTmpFFPPointObj.DATA.PRE_EXP_PNT, "P", true, true));		// 소멸예정
				$(".newUserBox").css("display", "");
			} catch (e) {
				log ("***** fn_controlCommonUserInfo EXCEPTION *****");
				log ("Error Message: " + e.message);
				log ("Error Code: " + e.number);
				log ("Error Name: " + e.name);
			}
		}

		// Top의 회원 Point 정보 갱신. 2015.10.07 FFP시스템개선
		function fn_updateTopUserPoint(balancePoint, totalPoint, expPoint) {
			//$("#memberInfo").html(jsLoginUserName + $.cfn_setText(jsonTopData, "name") + " " + $.cfn_currencyExp(balancePoint, "P", true, true));
			$("#gnb_mypage_index").html(jsTopLoginUserName + $.cfn_setText(jsonTopData, "name"));
			$("#gnb_refresh_index").html($.cfn_currencyExp(balancePoint, "P", true, true));
			$("#liFFPPoint").html($.cfn_currencyExp(balancePoint, "P", true, true));		// 총누적
			$("#ddBalance").html($.cfn_currencyExp(balancePoint, "P", true, true));	// 가용
			$("#ddExint").html($.cfn_currencyExp(expPoint, "P", true, true));		// 소멸예정
		}

		// Top 로그인 정보를 로컬의 로그인 정보에 셋팅한다.
		function fn_setLoginInfoToLocal() {
			if(typeof jsIsMember != "undefined") jsIsMember = jsTopIsMember;
			if(typeof jsLoginCheck != "undefined") jsLoginCheck = jsTopLoginCheck;
			if(typeof jsLoginUserID != "undefined") jsLoginUserID = jsTopLoginUserID;
			if(typeof jsLoginUserName != "undefined") jsLoginUserName = jsTopLoginUserName;
			if(typeof jsLoginUserNameL != "undefined") jsLoginUserNameL = jsTopLoginUserNameL;
			if(typeof jsLoginUserNameF != "undefined") jsLoginUserNameF = jsTopLoginUserNameF;
			if(typeof jsLoginEngUserNameL != "undefined") jsLoginEngUserNameL = jsTopLoginEngUserNameL;
			if(typeof jsLoginEngUserNameF != "undefined") jsLoginEngUserNameF = jsTopLoginEngUserNameF;
			if(typeof jsLoginFFPNo != "undefined") jsLoginFFPNo = jsTopLoginFFPNo;
			if(typeof jsLoginFFPGrade != "undefined") jsLoginFFPGrade = jsTopLoginFFPGrade;
			if(typeof jsLoginFFPGradeName != "undefined") jsLoginFFPGradeName = jsTopLoginFFPGradeName;
			if(typeof jsLoginFFPPoint != "undefined") jsLoginFFPPoint = jsTopLoginFFPPoint;
			if(typeof jsFFPGradeFromDate != "undefined") jsFFPGradeFromDate = jsTopFFPGradeFromDate;
			if(typeof jsFFPGradeToDate != "undefined") jsFFPGradeToDate = jsTopFFPGradeToDate;
			if(typeof jsFFPNextGradePoint != "undefined") jsFFPNextGradePoint = jsTopFFPNextGradePoint;
			if(typeof jsFFPNextGradeBoardCnt != "undefined") jsFFPNextGradeBoardCnt = jsTopFFPNextGradeBoardCnt;
			if(typeof jsFFPPointObj  != "undefined") jsFFPPointObj =jsTopFFPPointObj;
		}

	</script>


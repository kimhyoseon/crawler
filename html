			$.get(jsContextRoot + "/com/jeju/ibe/main/cmsMainTop.do", {device : "P",language_type:jsLang}, function(data) {
				//최상단 공지
				if(getCookie("openTopCookie") != "done"){
					var topNotice = data.topNotice || {};
					if(topNotice.POSTTYPE != ""){
						$("div.pageTopBanner").children().remove();
						var topNoti = "";
						if(topNotice.TXTCONTENT){
							// bg 컬러 변경
							if(topNotice.TXTBG != "DEFAULT"){
								$("div.pageTopBanner").attr("style","background-color:"+topNotice.TXTBG+" !important;");
							}

							topNoti = "&lt;div class='in'&gt;";
							if(topNotice.TXTCOLOR != "DEFAULT"){
								topNoti += "&lt;p style='color:"+topNotice.TXTCOLOR +"' !important; &gt;";
							}else{
								topNoti += "&lt;p&gt;";
							}

							topNoti+=topNotice.TXTCONTENT;
							if(topNotice.LINKURL){
								if(jsLang=="KR"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;자세히보기&lt;/a&gt;";
								}else if(jsLang=="EN"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;Detail&lt;/a&gt;";
								}else if(jsLang=="JP"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;詳細はこちら&lt;/a&gt;";
								}else if(jsLang=="CN"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;查看详细内容 &lt;/a&gt;";
								}else if(jsLang=="HK" || jsLang=="TW"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;查看詳細內容&lt;/a&gt;";
								}else if(jsLang=="TH"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;รายละเอียด  &lt;/a&gt;&lt;/p&gt;";
								}else if(jsLang=="VN"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;Xem chi tiết&lt;/a&gt;";
								}else if(jsLang=="RU"){
									topNoti +="&lt;a href='"+topNotice.LINKURL+"'&gt;Смотреть подробнее&lt;/a&gt;";
								}
							}
							topNoti+="&lt;/p&gt;";

							if(jsLang=="KR"){
								topNoti +="&lt;button class='bannerClose'&gt;배너 닫기&lt;/button&gt;";
							}
							else{
								topNoti +="&lt;button class='bannerClose'&gt;banner Close&lt;/button&gt;";
							}

							topNoti +="&lt;/div&gt;";
						}else{
							topNoti = topNotice.WCONTENT;
						}

						$("div.pageTopBanner").append(topNoti);
					}else{
						$("div.pageTopBanner").remove();
					}
				}else{
					$('div.pageTopBanner').slideUp(150);
				}
			});

			// 웹접근성(friver) - 20170116 스킵 네비게이션 스크립트 추가
			var skip = $("#JEJUAIR_SKIP_NAVI_DIV");
			skip.find('li').eq(1).find('a').attr('href','#gnb_wrap');

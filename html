			$("#btnAddSection").live("click", function(){
				if($("#hidFirstDepStnCity").val() != "" &amp;&amp; $("#hidLastArrStnCity").val() != ""){
					if($("#hidFirstDepStnCity").val() == $("#hidLastArrStnCity").val()){
						alert($.cfn_setText(jsonData, "msgSameCity"));
						return;
					}
				}
				if(jsSelectSectionSize &lt; jsMaxSection){
					$(".flightTab").animate({"height":263},150,"easeInOutQuint",function(){
						for (var i = 1; i &lt;= 2; i++){
							jsSelectSectionSize++;
							$("div[id=divRoute" + jsSelectSectionSize + "]").fadeIn(150);
						}

						if(jsSelectSectionSize == jsMaxSection){
							$("#divAddSection").css("display", "none");
						}
					});
					$(".mainTopWrap").animate({"margin-bottom":173},150,"easeInOutQuint");
				}else{
					alert("구간은 최대" + jsMaxSection + "개 까지 선택가능합니다.");
					return;
				}

				$("#btnRemoveSection").css("display", "");
			});

			//구간삭제 2015.12.04 연결발권
			$("#btnRemoveSection").live("click", function(){

				var prevSegDate = $('#txtDate' + (jsSelectSectionSize-2)).val(); //구간이 2개씩 줄어들기로함

				for (var i = 1; i &lt;= 2; i++) { //구간이 2개씩 줄어들기로함
					$("#hidDepStn"+jsSelectSectionSize).val("");
					$("#hidArrStn"+jsSelectSectionSize).val("");

					$("#hidDepName"+jsSelectSectionSize).val("");
					$("#hidArrName"+jsSelectSectionSize).val("");

					$("#hidCountry"+jsSelectSectionSize).val("");

					$('#txtDate'+jsSelectSectionSize).val(prevSegDate) // YYYY-MM-DD

					$("#txtDepStn"+jsSelectSectionSize).val($.cfn_setText(jsonData, "lblDep"));		// 출발지
					$("#txtArrStn"+jsSelectSectionSize).val($.cfn_setText(jsonData, "lblArr"));		// 도착지

					$("#hidLastArrStnCity").val("");

					$('#txtDate'+jsSelectSectionSize).val(prevSegDate);
					$('#txtDate'+jsSelectSectionSize).data('initDate',prevSegDate.replace(/[^0-9]/gi,''));
					$('#txtDate'+jsSelectSectionSize).data('initDate1',prevSegDate);
					$('#txtDate'+jsSelectSectionSize).data('selectDate',prevSegDate.replace(/[^0-9]/gi,''));
					$('#txtDate'+jsSelectSectionSize).data('selectDate1',prevSegDate);

					jsRouteTypeArray.splice(jsSelectSectionSize, 1);

					$.each( $(jsRouteTypeArray), function(idx){
						if(jsRouteTypeArray[idx] == "I") {
							$("#hidRouteType").val("I"); //하나라도 국제선이면 국제선운행
							return false;
						} else {
							$("#hidRouteType").val("D");
						}
					});

					if(jsSelectSectionSize &gt; 1){
						$("div[id=divRoute" + jsSelectSectionSize + "]").css("display", "none");
						jsSelectSectionSize--;
					}
				}

				jsRetDate = $("#txtDate" + jsSelectSectionSize).val().replace(/[^0-9]/gi,'');

				$("#divAddSection").css("display", "");

				$(".flightTab").animate({"height":170},150,"easeInOutQuint");
				$(".mainTopWrap").animate({"margin-bottom":81},150,"easeInOutQuint");

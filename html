			// 웹접근성(Tocanz) - 20151103 - Kang
			// 변경 내용: 기존 button 형식에 checkbox 를 input type checkbox 로 변경 처리함
			$('input[type="radio"][name="ckA"]').click(function() {

				// 출/도착지 레이어가 오픈 상태일 경우
				var openId;

				$("button[name=ibtnOpen], div[name=ibtnOpen]").each(function(){
					openId = $("button[name=ibtnOpen], div[name=ibtnOpen]").attr("id");

					$("#"+openId).attr("name", "ibtnClose");
					$("#"+openId).removeClass("open");
					deleteLayer();
				});

				//animate
				var idx = $(".dep3Tab").index(this);
				//20160513 예매 영역 리뉴얼
				if(idx == 2){
					$(".flightTab").animate({"height":170},150,"easeInOutQuint");
					$(".mainTopWrap").animate({"margin-bottom":81},150,"easeInOutQuint");
				}else{
					$(".flightTab").animate({"height":120},150,"easeInOutQuint");
					$(".mainTopWrap").animate({"margin-bottom":30},150,"easeInOutQuint");
				};

				// 웹접근성(Tocanz) - 20151123 - Kang
				$('input[id^="btnTrip"]').removeClass("active");
				$('input[id^="btnTrip"]').parent().removeClass("active");
				$(".dep3TabCon").hide();

				$(this).addClass("active");
				$(this).parent().addClass("active");
				jQuery(".dep3TabCon").eq(idx).show();

				//alert("id : "+$(this).attr('id')+", value : "+$(this).attr('value'));
				var trip = "";

				// 변경 전 TripType 으로 데이터 구성
				if ($('#hidTripType').val() == "R") {
					trip = "RT";
				} else if ($('#hidTripType').val() == "M") {
					trip = "MT";
				} else if ($('#hidTripType').val() == "O") {
					trip = "OW";
				}

				var conHtml = $('#div'+trip).html();
				//log("conHtml : " + conHtml);
				$('#div'+trip).html("");

				$('#hidTripType').val($(this).attr('value'));		// TripType 데이터 값 변경
				jsTripType = trip;

				jsNationCode = "";

				if ($(this).attr('id') == "btnTripSingle") {

					ga('rollup.send', 'event', 'PC_MAIN', 'Booking_type', 'One');
					n_click_logging('https://www.jejuair.net/jejuair/userclick.jsp?jcategory=PC_MAIN&amp;jaction=Booking_type&amp;jlabel=One',location.href);

					$('#divOW').html(conHtml);

					$('.mPay').show();
					$('#routeDiv').css('top', '');
					//20160513 예매 영역 리뉴얼
					//$('#divRoute2, #divPaxBox, #divBtnAgeCalc, #divBtnReservation').switchClass('mt5', 'mt10');

					$('#divDepStn1').after($("#divArrStn2"));

					$('#divRoute3').css("display", "none");
					$('#divRoute4').css("display", "none");

					$('#divAddSection').css("display", "none");

					$('#divArrStn1').css("display", "none");
					$('#divDepStn2').css("display", "none");
					$('#divMultiSection3').css("display", "none");

					//20160513 예매 영역 리뉴얼
					//$("#divSelADT").removeClass("w130");
					//$("#divSelCHD").removeClass("w130");
					//$("#divSelINF").removeClass("w130");
					//$("#divBtnAgeCalc").removeClass("w130");
					//$("#divBtnReservation").removeClass("mr10");

					//$("#divPaxBox").addClass("tr");
					//$("#divSelADT").addClass("w85");
					//$("#divSelCHD").addClass("w85");
					//$("#divSelINF").addClass("w85");
					//$("#divBtnAgeCalc").addClass("w85");

					$('#depDateCal').attr("title",$.cfn_setText(jsonData,"depDateCal.title")); //출발일 날짜 선택 달력
					$('#retDateCal').attr("title",$.cfn_setText(jsonData,"retDateCal.title")); //도착 날짜 선택 달력

					jsMultiFlag = "";
					jsRouteType2 = "";
					jsStnId = "txtDepStn1";		// 구간1 출발지를 조회하기 위함

					jsSelectSectionSize = 1;

					// 출발지 조회조건 세팅
					$.cfn_airportSearchTerms();

				} else if($(this).attr('id') == "btnTripReturn") {

					 ga('rollup.send', 'event', 'PC_MAIN', 'Booking_type', 'Round');

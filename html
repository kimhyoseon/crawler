	// 인덱스 화면 가기. 2015.08.25 FFP시스템개선
	$("#gnb &gt; li.dep1 &gt; h2 &gt; a").click(function(e) {
	  // 이벤트 중지
	  e.preventDefault();
    e.stopPropagation();

    var self_id      = $(this).attr("id") || "";
    var cursor_style = $(this).css("cursor") || "default";

    if ( "" == self_id ) {
      return;
    } else if ( "default" == cursor_style ) {
      return;
    }

    log("self_id ==&gt; " + self_id);
	  log("hasClass.dep1 ==&gt; " + $(this).parents("li").hasClass("dep1"));
    log("common_gnb ==&gt;" + common_gnb.gnb_map["top"][self_id]);
    log("typeof common_gnb ==&gt;" + typeof common_gnb.gnb_map["top"][self_id]);
    log("jsLang ==&gt; "+jsLang);

    if ( typeof common_gnb.gnb_map["top"][self_id] != "undefined") {
      if ( $(this).parents("li").hasClass("dep1") ) {
        var tgURL = jsReturnHost +common_gnb.gnb_map["top"][self_id];

        // 해외 사이트의 경우 서비스안내, 이벤트, 여행상품 인덱스 페이지는 보여주지 않는다.
        if (jsLang != "KR") {
          if (self_id != "c_one_depth0" &amp;&amp; self_id != "d_one_depth0" &amp;&amp; self_id != "e_one_depth0" ){
            jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
          }
        } else {
          jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
        }
      }
    }
	});

	//!--- gnb menu start !!! ---
	$("ul.dep2Box &gt; li.dep2 &gt; a, ul.dep3Box &gt; li.dep3 &gt; a").click(function(e) {
		e.preventDefault();
		e.stopPropagation();
        var self_id      = $(this).attr("id") || "";
        var cursor_style = $(this).css("cursor") || "default";

        if ( "" == self_id ) {
            return;
        } else if ( "default" == cursor_style ) {
            return;
        }

        if ( $(this).parent().hasClass("dep2") ) {
        	if ( "JPG10A00" == self_id || "JPG20A00" == self_id || "JNE70100" == self_id ) {
        		window.open(common_gnb.gnb_map[self_id][self_id], "_blank");
        	} else {
            //document.location.href = common_gnb.gnb_map[self_id][self_id];
            if(self_id.substring(0, 3)=="JMP" &amp;&amp; self_id.charAt(3)!="1"){
            	if(typeof jsIsMember == "undefined") jsIsMember = IsMember;
            	if(loginChk=="false"){
            		if("gnb_noLogin"==jsIsMember || "false" == IsLoginCheck)alert($.cfn_setText(common_gnb.json_data, "msgAlert12"));
            	}
            }

        		var tgURL = jsReturnHost +common_gnb.gnb_map[self_id][self_id];
        		jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
        	}
        } else if ( $(this).parent().hasClass("dep3") ) {
        	var parent_id = $(this).parent().parent().siblings("a").attr("id") || "";
        	if ( "" == parent_id ) {
        		return;
        	} else if ( "JSI30800" == self_id ) {
        		alert("기내식 예약 서비스는 준비 중 입니다.");
        		return;
        	} else if ( "JPG10A00" == parent_id || "JPG20A00" == parent_id || "JPG50102" == self_id || "JPG50103" == self_id) {
        		window.open(common_gnb.gnb_map[parent_id][self_id], "_blank");
        	} else {
            //document.location.href = common_gnb.gnb_map[parent_id][self_id];
        		var tgURL = jsReturnHost + common_gnb.gnb_map[parent_id][self_id];
        		jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
        	}
        }
	});

	$("#more_newslist, #more_event").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		if ("" == site_id) {
            return;
        }
		//document.location.href = common_gnb.site_map[site_id];
		var tgURL = jsReturnHost + common_gnb.site_map[site_id];
		jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
	});

	// 디자인 변경. 2015.10.06 FFP시스템개선
	$("div.snsCenter &gt; ul &gt; li &gt; a").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		if ("" == site_id) {
            return;
        }
		//document.location.href = common_gnb.site_map[site_id];
		window.open(common_gnb.site_map[site_id], "_blank");
	});

	$("div.sitemap &gt; ul &gt; li").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		site_id = site_id.split("SMAP").join("");

		if ("" == site_id) {
            return;
        } else if ("default" == cursor_style) {
            return;
        }

        if ( site_id.indexOf("JPG20A") != -1 ) {
        	window.open(common_gnb.gnb_map["JPG20A00"][site_id], "_blank");
        }else if ( site_id.indexOf("JPG50101") != -1 || site_id.indexOf("JPG50102") != -1 || site_id.indexOf("JPG50103") != -1 ) {
        	window.open(common_gnb.gnb_map["JPG50100"][site_id], "_blank");
		}else{
			//document.location.href = common_gnb.site_map[site_id];
			var tgURL = jsReturnHost + common_gnb.site_map[site_id];
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}
	});
	//!--- gnb menu period !!! ---

	//!--- gnb google search start !!! ---
	$("input[name='goGoogleSearch']").click(function(e) {
		e.preventDefault();
		//e.stopPropagation();

		$("#frmTotalSearch").attr("action", "http://" + window.location.host + "/jejuair/etc/total_search.jsp").submit();
	});
	//!--- gnb google search period !!! ---


	$("div.flightLink &gt; a").click(function(e) {
		e.preventDefault();
		var site_id = $(this).attr("id").replace("main_", "");
		//document.location.href = common_gnb.site_map[site_id];
		var tgURL = jsReturnHost + common_gnb.site_map[site_id];
		jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
	});

	//!--- bottom menu start !!! ---
	// 퍼블리싱 변경. 2015.10.12 FFP시스템개선
	$("div.footerInfo &gt; ul &gt; li, div.footerlogo &gt; ul &gt; li").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		if ("" == site_id) {
            return;
        } else if ("default" == cursor_style) {
            return;
        }
        //[인터넷부킹고도화]
		if ( "jejuair_recruit" == site_id ) {
			//window.open(common_gnb.gnb_map["bottom"][site_id], "_blank");
			var tgURL = "http://recruit.jejuair.net/main.jsp";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_blank"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		} else if ( "dangerous_goods" == site_id ) {
			//window.open(common_gnb.gnb_map["bottom"][site_id], "_blank");
			var tgURL = "https://avsec.ts2020.kr/avsc/main.do?refererPara=JJA2";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_blank"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		} else if ( "contingency_plan" == site_id || "customer_service_plan" == site_id || "contingency_plan" == site_id || "services_fees" == site_id) {
				window.open(common_gnb.gnb_map["bottom"][site_id], "_blank");
		} else if ( "company_inverstment" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/inverstment/stock.do";
//			var tgURL = "http://twww.jejuair.net/jejuair/inverstment/stock.jsp";      // TEST
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		} else if ( "transport_stipulation" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/login/agreement/transportbottom.do";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else if ( "homepage_stipulation" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/login/agreement/homepage.do";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else if ( "privacy_policy" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/login/agreeinfo/pre_policy.do";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else if ( "site_map" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/etc/sitemap.do";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else if ( "service_plan" == site_id ) {
			var tgURL = jsReturnHost +"/jejuair/kr/login/agreement/service_plan.do";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else if ( "Application_pdf" == site_id ) {
			var tgURL = "http://www.jejuair.net/jejuair/etc/pdf/Application_for_Damage_Redress.pdf";
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_blank"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}else {
			//document.location.href = common_gnb.gnb_map["bottom"][site_id];
			var tgURL = jsReturnHost + common_gnb.gnb_map["bottom"][site_id];
			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		}
	});

	// iOS, Android App Download Link - 2015.11.02
	$("div.familysite &gt; div.appDownload &gt; a").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		if ("" == site_id) {
            return;
        }

		window.open(common_gnb.site_map[site_id], "_blank");
	});


});

$.fn.extend({
	/*-----------------------------------------
	블랭크 체크
	-------------------------------------------*/
	checkBlank : function(msg){
		var ObjValue = this.val();
		ObjValue = $.trim(ObjValue);
		if(ObjValue != ''){
			return true;
		}else{
			alert(msg);
			$(this).focus();
			return false;
		}
	}

});

//[인터넷부킹고도화] Top Menu 높이값 재 설정하는 함수. 2015.09.24 FFP시스템개선
function gnb_reset_mask() {
  /* 마스크 높이값 설정 */
	/* 높이는 css 에서 제어하기 때문에 삭제 합니다.
  $(".dep2").removeAttr("style");
  $(".subMask").removeAttr("style").each(function(){
    var one_height = $(this).find(".maskCon").height(),
      two_height = one_height - 130;
    $(this).height(one_height);
    // 2015.09.22 FFP시스템개선
    if ( !$(this).find('.m6').length ) {
      $(this).find(".dep2").height(two_height)
    }
  }).hide();
  */
}

//[인터넷부킹고도화]
$(".subMask").hide();
$(".dep1 h2 a").on("mouseenter focus",function(){
	var idx = $(".dep1 h2 a").index(this);

	//초기화
	$(".dep1 h2 a").removeClass("active");
	$(".dep1 .arr").hide();
	$(".subMask").hide();
	$(".maskCon").css("top",0);

	//클릭 설정
	$(this).addClass("active");
	$(this).prev().show();
	$(".subMask").eq(idx).show();

});
//[인터넷부킹고도화]
$("#gnb").on("mouseleave",function(){
	$(".blackLightbox1").remove();

	$(".dep1 h2 a").removeClass("active");
	$(".dep1 .arr").hide();

});

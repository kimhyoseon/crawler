	$("#gnb_logout").click(function(e) {
		e.preventDefault();
		//document.location.href = "/jejuair/common/logout.jsp?target_url=/jejuair/main.jsp";
        if(common_gnb.logout_xhr &amp;&amp; common_gnb.logout_xhr.readyState != 4){
        	common_gnb.logout_xhr.abort();
        }
        common_gnb.logout_xhr = $.post("/jejuair/common/logout_language.jsp", function(data){

      // 디자인 변경. 2015.09.20 FFP시스템개선
			$("#gnb_login").show();
			$(".loginWrap").hide();

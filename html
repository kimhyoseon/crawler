			if(settings.url =="/jejuair/com/jeju/ibe/main/managing_cms_list.do"){
				function goga(item){
					var temp = "Find us button"+item;
					//ga('send','event','Icon',temp);
				}



				//오늘하루 그만보기
				$(document).on("click","#todayHide",function(e){
					e.preventDefault();
					$(this).addClass("active");
					$(this).parent().addClass("active");

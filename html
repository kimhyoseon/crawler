	var darkStatus ="";
		var darkUserFlag ="";


		var prevUrl = "null";
		//다크사이트 상황 처리 2016.05.10
		$.ajax({
			async : false,
			type : "get",
			    dataType : "json",
				url : "/jejuair/com/jeju/ibe/dark/status.do",
				data:{device:"P",url:prevUrl,language_type:jsLang},
				success : function(data) {
					darkStatus = data.darkyn;
					darkUserFlag = data.userflag;
						if(darkStatus!="N"){
		 					if(darkStatus=="Y" ){
		 						if(!darkUserFlag){
		 							location.href = "https://www.jejuair.net/jejuair/dark/main.jsp";
		 						}
		 					}
		 				}

				},
				error : function(request,status,error){
				alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
			}
		});// end of ajax


		var jsLangTypeLowerCase = "kr";
		var jsLangUrl = "kr"


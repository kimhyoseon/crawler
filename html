					jsSelectSectionSize = 2;

					// 출발지 조회조건 세팅
					$.cfn_airportSearchTerms();

				} else if($(this).attr('id') == "btnTripMulti") {

					ga('rollup.send', 'event', 'PC_MAIN', 'Booking_type', 'Multi');
					n_click_logging('https://www.jejuair.net/jejuair/userclick.jsp?jcategory=PC_MAIN&amp;jaction=Booking_type&amp;jlabel=Multi',location.href);

					$('#divMT').html(conHtml);

					$('#btnPayTypeCommon').trigger('click');
					$('.mPay').hide();
					//20160513 예매 영역 리뉴얼

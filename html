				$.extend(common_gnb.json_data, jsonTopData);
				// 가져온 텍스트 화면에 설정
				$.cfn_setProgramScreenText(jsonTopData);
			}

			if($.cfn_getProgramScreenText === undefined) { // TODO: 의존성 결핍으로 인한 plugin 로드가 되지 않는 버그를 방지. 향후 의존성 관리를 통해 개선되어야 함.
				setTimeout(parseScreenText,100);
			} else {
				parseScreenText();
			}


			 			for(var idx in socialList){
			 				if(socialList[idx].IMAGETITLE){
			 					var social = "&lt;li onclick=\"ga('rollup.send', 'event', 'PC_MAIN', 'Link_sns', '"+socialList[idx].IMAGETITLE+"'); n_click_logging('https://www.jejuair.net/jejuair/userclick.jsp?jcategory=PC_MAIN&amp;jaction=Link&amp;jlabel="+socialList[idx].IMAGETITLE+"',location.href);\"&gt;";
			 					if(jsLang=="KR"){
			 						social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+socialList[idx].LINKURL+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" 새창열림 ' &gt; ";
			 					}else{

			 						//페이스북 링크 수정 , 다국어 EN (말레이시아,필리핀)의 경우 컨트리셋션 값으로 페이스북 링크 분기 (CMS상 필리핀링크,말레이시아링크 순으로 입력필요 ',' 구분)
			 						//social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+socialList[idx].LINKURL+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" new Open Window' &gt; ";
			 						var linkURL;
			 				 		if(socialList[idx].LINKURL.indexOf(",") &gt; 1){
			 				 			linkURL = socialList[idx].LINKURL.split(",");
				 						if(jsCountryType=="PHIL"){
					 						social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+linkURL[0]+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" new Open Window' &gt; ";
				 						}else if(jsCountryType=="MYS"){
					 						social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+linkURL[1]+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" new Open Window' &gt; ";
				 						}else{
					 						social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+linkURL[0]+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" new Open Window' &gt; ";
				 						}
			 				 		}else{
				 						social +="&lt;a id='mainsns"+(parseInt(idx)+1)+"' href='"+socialList[idx].LINKURL+"' target='_blank' title='"+socialList[idx].IMAGETITLE+" new Open Window' &gt; ";
			 				 		}
			 					}
			 					social +="&lt;img src='http://img.jejuair.net/jejuair"+socialList[idx].IMAGEPATH+"' alt='"+socialList[idx].IMAGEMOD+"' /&gt;";
			 					social +=socialList[idx].IMAGETITLE+"&lt;/a&gt;&lt;/li&gt;";
			 					$("ul.sns").append(social);
			 				}
			 			}
			 		}
			 		//팝업 공지
			 		// 다국어 처리하기.
					var popup = data.selectPopupYN || {};
					if(popup){
						if(popup.USEYN =="Y"  &amp;&amp; popup.TXTTITLE){
							if(getCookie("openPopCookie") != "done"){
								openLayerPop(this,445,350,"/jejuair/popup/mainNotice.jsp","","","main");
							}
						}
					}
			 	});
			});



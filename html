			$('input[id^="btnPayType"]').click(function() {
				$('input[id^="btnPayType"]').removeClass("active");
				$('input[id^="btnPayType"]').parent().removeClass("active");
				$(this).addClass("active");
				$(this).parent().addClass("active");
				$('#hidPayType').val($(this).attr('value'));		// TripType 데이터 값 변경
			});

			//--------------------------------------
		 	// 왕복/편도/다구간 선택 시 화면 및 조회 조건을 변경한다.

		//document ready : E
		}).ajaxSuccess(function(event,xhr,settings){

			if(settings.url.substring(0, 40)=="/jejuair/com/jeju/ibe/main/cmsMainTop.do"){
				//최상단 배너 닫기
				$('.pageTopBanner .bannerClose').on('click',function(e){
					e.preventDefault();
					setCookie("openTopCookie","done",1);
					$('.pageTopBanner').slideUp(150);
				});
			}

		});

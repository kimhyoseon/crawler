				$(".dep3TabCon").eq(idx).show();

				// 탑승객 인원(성인, 소아, 유아) 선택 시 GA TAG EVENT 실행
				// 여정타입 변경 시 로드되지 않아 다시 선언
				$("#spanSelADT, #btnSelADT").on("click", function() {
					fn_gaEventPax("Adult");
				});
				$("#spanSelCHD, #btnSelCHD").on("click", function() {
					fn_gaEventPax("Child");
				});
				$("#spanSelINF, #btnSelINF").on("click", function() {
					fn_gaEventPax("Infant");
				});
			});


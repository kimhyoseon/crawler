		var jsAirplaneDrawing = false;
		var browserVer = getBrowserVersion();
		var svg;
		var bar;

		function svgGraph() {
			var s = Snap('#SVG')
				, circle1 = s.circle(80, 80, 60).attr('id', 'circle1')
				, circle2 = s.circle(80, 80, 60).attr('id', 'circle2')
				, circle3 = s.path("M80,20 a60,60 0 1,1 0,120 a60,60 0 1,1 0,-120").attr('id', 'circle3')
				, airplane = s.g().attr({ id : 'airplane' })
				, airplaneImg = airplane.image("/jejuair/images/renewal/90/ico_airplane1.png", 47,10,49,24)
				, len = circle3.getTotalLength()
				, data = $('.graphData')
				, dataTotal = parseInt(data.find('.total').html().replace(/,/g,''))
				, dataCurrent = parseInt(data.find('.current').html().replace(/,/g,''))
				, dataPercentage = (dataCurrent/dataTotal)*100
				, mTime = 3 * (dataPercentage/100)

			function init() {
				data = $('.graphData');
				dataTotal = parseInt(data.find('.total').html().replace(/,/g,''));
				dataCurrent = parseInt(data.find('.current').html().replace(/,/g,''));
				dataPercentage = (dataCurrent/dataTotal)*100;

				data.find('.current').html('0');

				circle1.attr({ fill: "none", stroke: "#cacbce", strokeWidth: 5});
				circle2.attr({ fill: "none", stroke: "#fff",	strokeWidth: 12, transform : 'rotate(-90deg)' });
				circle3.attr({ fill: "none", stroke: "transparent",	strokeWidth: 0	});
				TweenMax.set($('#circle2'), { drawSVG : '0%' });
				airplane.attr({	opacity : 0	});

			}

			function drawLine() {
				if ( dataPercentage &gt; 0 ) {

					if ( dataPercentage &gt; 92.5 ) dataPercentage = 92.5;
					mTime = 3 * ((dataPercentage)/100);

					TweenMax.to($('#circle2'), mTime, { drawSVG:'100%', ease:Power1.easeInOut });
					TweenMax.to($('#circle2'), mTime, { drawSVG:dataPercentage + '%', ease : Linear.easeNone });
					data.find('.current').animateNumbers(dataCurrent, true, mTime*1000);
					airplane.attr({	opacity : 1	});
					airplane.drawAtPath(circle3, mTime*1000, { percent : dataPercentage, addPlus : 12 });
				} else {
					// 포인트가 없을 경우에 비행기 한바퀴 돌리기. 2015.11.16 FFP시스템개선
					mTime = 3;
					airplane.attr({	opacity : 1	});
					airplane.drawAtPath(circle3, mTime*1000, { percent : 100, addPlus : 12 });
				}
			}

			return {
				draw : drawLine
				, init : init
			}
		}
		//웹접근성(Tocanz) - 20151215 Kang
		function RemoveHTML(text) {
			var objReg = new RegExp();  //  정규 표현식 객체를 생성한다
		    objReg = /[&lt;][^&gt;]*[&gt;]/gi;
			text= text.replace(objReg, "");
			return text;
		}
		function barGraph() {

		  function init() {
				var wrap = $('.graph6');

				wrap.each(function(){
					var _this = $(this)
					, bar = wrap.find('.bar')
					, air = wrap.find('.air')
					, ico = bar.find('i')
					, txt = wrap.find('.current')
					, txtNum = parseInt(txt.html().replace(/,/g,''))
					, totalNum = parseInt(wrap.find('.total').html().replace(/,/g,''))

					bar.css('width', 0);
					ico.hide();

					txt.html('0');

					_this.data({
						txtNum : txtNum
					});

					air.hide().animate({
						left : 0
					}, 0);
					//air.hide();

				});

			}

			function motion() {

			  var wrap = $('.graph6');

				wrap.each(function() {

					var	_this = $(this)
						, bar = _this.find('.bar')
						, air = _this.find('.air')
						, ico = bar.find('i')
						, txt = _this.find('.current')
						, txtNum = _this.data('txtNum')
						, totalNum = parseInt(_this.find('.total').html().replace(/,/g,''))
						, percent = (txtNum/totalNum)*100
						, mTime = 3000 * (percent/100)

					if ( percent &gt; 0 ) {

						if ( percent &gt;= 87 ) percent = 87;

						mTime = 3000 * (percent/100)

						bar.animate({
							width : percent + '%'
						}, mTime);

						air.show().animate({
							left : percent + '%'
						}, mTime);

						ico.fadeIn(300);
						txt.animateNumbers(txtNum, true, mTime);
					} else {
						air.show().animate({
							left : 0
						}, mTime);
					}
				});
			}

			return {
				init : init
				, draw : motion
			}

		}


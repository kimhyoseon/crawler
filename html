		//--------------------------------------
	 	// Banner의 ga태그 체크를 위해 event_id를 파싱 2014.11.07
	 	//--------------------------------------
		function bannerGAChk(addr, flag) {
			var pattern1 = /event_id=[0-9]{1,}&amp;/;
			var re1 = new RegExp(pattern1);

			if (re1.test(addr) ) {
				var tmpId = re1.exec(addr)[0];
				var pattern2 = /[0-9]{1,}/;
				var re2 = new RegExp(pattern2);
				if (re2.test(tmpId) ) {
					eventID = re2.exec(tmpId)[0];
				}
			}

			if (eventID != "") {
				if (flag == "event") {
					//ga('send', 'event', 'Event', 'Event banner', eventID);
				} else if (flag == "main") {
					//ga('send', 'event', 'Main', 'Banner', eventID);
				}
			}

			log("==" + eventID + "/" + flag);
		}

		//웹접근성(Tocanz) - 20151208 - Kang
		function bannerGAChk2(addr, flag) {

			var pattern1 = /event_id=[0-9]{1,}&amp;/;
			var re1 = new RegExp(pattern1);
			var eventID = "";

			if (re1.test(addr) ) {
				var tmpId = re1.exec(addr)[0];
				var pattern2 = /[0-9]{1,}/;
				var re2 = new RegExp(pattern2);
				if (re2.test(tmpId) ) {
					eventID = re2.exec(tmpId)[0];
				}
			}
			if (eventID != "") {
				if (flag == "event") {
					//ga('send', 'event', 'Event', 'Event banner', eventID);
				} else if (flag == "main") {
					//ga('send', 'event', 'Main', 'Banner', eventID);
				}
			}
			window.location.href = addr;
			log("==" + eventID + "/" + flag);
		}

		//--------------------------------------
	 	// 사용자가 상단에서 로그인 했을 경우 렌터카&amp;호텔 조회 가능하게 처리 2014.08.12
	 	//--------------------------------------
		function fn_setSession(){
	    	$.post("/jejuair/com/jeju/ibe/login/join/chkLogin.do", function(data) {
	    		jsLoginCheck = "true";

				//나의 포인트 항목 데이터 세팅
				//등급
				$("#ddUserFFPGrade").html(jsLoginFFPGradeName);

				//가용포인트
				var jsFFPPointObj = $.cfn_searchFFPPoint("BQ", jsLoginFFPNo, "JPNT");
				if (jsFFPPointObj.DATA != undefined) {
					jsLoginFFPPoint = jsFFPPointObj.DATA.BALANCEPOINT;	//가용포인트
					jsAccumulatePoint = jsFFPPointObj.DATA.ACC_POINT_3;	//누적포인트

					$("#ddUserFFPPoint").html($.cfn_currencyExp($.cfn_searchFFPPoint("CQ", jsTopLoginFFPNo, "JPNT").DATA.BALANCEPOINT, "P", true, true));
					$("#ddPointInfoFFPPoint").html($.cfn_setComma(jsLoginFFPPoint) + "&lt;span&gt;P&lt;/span&gt;");
					//등급 누적포인트
					$("#ddAccumulatePoint").html($.cfn_setComma(jsAccumulatePoint) + " P");

					// 등급 누적 포인트의 자릿수가 5자리보다 많은 경우 적용 클래스 변경
					if (String(jsAccumulatePoint).length &gt; 5) {
						$("#ddAccumulatePoint").removeClass("no");
						$("#ddAccumulatePoint").addClass("no1");
					}

					//현재누적포인트, 등업시필요포인트
					$("span[name='spCurrentPoint']").html($.cfn_setComma(jsAccumulatePoint));
					$("span[name='spTotalPoint']").html($.cfn_setComma(jsFFPNextGradePoint));
				}
			});

	    	//[인터넷부킹고도화]
			$(".mPointOut").hide();
			$("div.mTab.mPoint").show();
		}

		//--------------------------------------
	 	// 출/도착지 엔진 호출
	 	//--------------------------------------
		function fn_searchAirport( searchTermsObj ) {
			//log("fn_searchAirport : " + JSON.stringify(searchTermsObj.searchJson));
			searchTermsObj.searchJson.SystemType = "IBE"; // add by dark

			//다구간인경우에 이전출발지는 다음출발지가 될 수 없다.
			if(jsMultiFlag == "Y") {
				searchTermsObj.searchJson.TrnType = "D";
				searchTermsObj.searchJson.AddOnBlock = "Y";
			} else if ($('#hidTripType').val() == "O") {
				searchTermsObj.searchJson.TrnType = "D";
			}

			$.ajax({
				async : false,
				type : "get",
	 		    dataType : "json",
	 		    url : "http://ibsearch.jejuair.net/jejuair/com/jeju/ibe/getAirport.do",
	 			//url : jsContextRoot+"/com/jeju/ibe/getAirport.do",
	 			data : searchTermsObj.searchJson,
	 			success : function(data) {
 					//log("data.Result &gt; " + JSON.stringify(data.Result) );
	 				if(data.Result.code == '0000'){
	 					//fn_createAirpotTbl(data.Result.data);

	 					if ( jsStnId.indexOf("DepStn") != -1 ) {

							depStnJsonArray[jsStnId.substring(9)] = data.Result.data;

	 					} else if ( jsStnId.indexOf("ArrStn") != -1 ) {

	 						arrStnJsonArray[jsStnId.substring(9)] = data.Result.data;

	 					}



	 					if ( jsStnId.indexOf("DepStn1") != -1 ) {
	 						depStnJsonArray[1] = data.Result.data;
	 					} else if ( jsStnId.indexOf("ArrStn1") != -1 ) {
	 						arrStnJsonArray[1] = data.Result.data;
	 					} else if ( jsStnId.indexOf("DepStn2") != -1 ) {
	 						depStnJsonArray[2] = data.Result.data;
	 					} else if ( jsStnId.indexOf("ArrStn2") != -1 ) {
	 						arrStnJsonArray[2] = data.Result.data;
	 					}
	 				} else {
	 					alert(data.Result.message + "\n\n"+ "[에러코드 : "+data.Result.code+"]");
	 				}
	 			},
	 			error : function(request,status,error){
					alert("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
				}
			});	// end of ajax
		} // fn_searchAirport

		//--------------------------------------
	 	// SelectBox Init
	 	//--------------------------------------
		function fn_selSectionInit() {
			//log("fn_selSectionInit");
			$("#txtDepStn1").val($.cfn_setText(jsonData, "lblDep"));		// 출발지
			$("#txtArrStn1").val($.cfn_setText(jsonData, "lblArr"));		// 도착지
			$("#txtDepStn2").val($.cfn_setText(jsonData, "lblDep"));		// 출발지
			$("#txtArrStn2").val($.cfn_setText(jsonData, "lblArr"));		// 도착지
			$("#txtDepStn3").val($.cfn_setText(jsonData, "lblDep"));		// 출발지
			$("#txtArrStn3").val($.cfn_setText(jsonData, "lblArr"));		// 도착지
			$("#txtDepStn4").val($.cfn_setText(jsonData, "lblDep"));		// 출발지
			$("#txtArrStn4").val($.cfn_setText(jsonData, "lblArr"));		// 도착지

			$("#hidDepStn1").val("");
			$("#hidArrStn1").val("");
			$("#hidDepStn2").val("");
			$("#hidArrStn2").val("");

			$("#hidRouteType").val("");
			$("#hidCountry1").val("");
			$("#hidCountry2").val("");
			$("#hidCountry3").val("");
			$("#hidCountry4").val("");

 			jsRouteType = "";
		}	// fn_selSectionInit()

		//--------------------------------------
	 	// 달력 세팅
	 	//--------------------------------------
		function fn_setCalendar() {
			var dateFormat2 = 'yy-mm-dd';	// 기본

			$.datepicker.setDefaults({
			 	dateFormat: dateFormat2,
			 	prevText: jsPrevText,
			 	nextText: jsNextText,
			 	monthNames: jsArrMonth,
			 	monthNamesShort: jsArrMonth,
				dayNames: jsArrDay,
			 	dayNamesShort: jsArrDay,
			 	dayNamesMin: jsArrDay,
			 	isRTL: false,
			 	showMonthAfterYear: true,
			 	minDate: 0,
			 	maxDate: 330, // 현재 날짜로 부터 예약 가능일자 [ 현재 날짜 ~ 330일 ]
			 	changeMonth: true,
			 	changeYear : true,
			 	inline: true
			});

			var date = $.format(jsCurrentDateObj, dateFormatValue1);

			$.each($("button[id^=dateCal]"), function(idx) {

				var index = idx + 1;

				$('#txtDate' + index).val( date );
				$('#txtDate' + index).data('initDate', jsCurrentDate);
				$('#txtDate' + index).data('initDate1', date);
				$('#txtDate' + index).data('selectDate', jsCurrentDate);
				$('#txtDate' + index).data('selectDate1', date);
				$('#txtDate' + index).data('restDate', "");
				$('#txtDate' + index).data('restDate1', "" );
				$('#txtDate' + index).data('weekArea', "" );
				//jsDepDate = $("#txtDepartureDate").val().replace(/[^0-9]/gi,'');

				$(this).click(function(){

					var fromIndex = index;
					var toIndex = index+1;

					if ( $('#hidTripType').val() == "R" ){
						fromIndex = 1;
						toIndex = 2;
					}
					if(index == 1){
						fromIndex = 1;
					}
					if(index == jsMaxSection){
						toIndex = jsMaxSection;
					}


					$("#gWrap").append('&lt;div id="calandarSpace" &gt;&lt;/div&gt;&lt;div id="divCalandarSpace" &gt;&lt;/div&gt;');
					doubleData = new Object();
					doubleData.initDate    = $('#txtDate' + index).data('initDate1');
					doubleData.from        = '#txtDate' + (fromIndex);
					doubleData.to          = '#txtDate' + (toIndex);
					doubleData.target      = $(this);
					doubleData.callBack    = calComplete;
					doubleData.dateFormat  = dateFormatValue1;
					doubleData.isWeek      = false;
					doubleData.chkWeekend  = true;
					doubleData.depWeekArea = $("#txtDate" + fromIndex).data("weekArea");
					doubleData.retWeekArea = $("#txtDate" + toIndex).data("weekArea");
					if ( $('#hidTripType').val() == "M" ){
						doubleData.depHeader   = $.cfn_setText(jsonData, "lblSection")+index;		// Text : 구간
					} else {
						doubleData.depHeader   = $.cfn_setText(jsonData, "depDate");		// Text : 가는날
					}

					doubleData.arrHeader   = $.cfn_setText(jsonData, "retDate");		// Text : 오는날
					doubleData.btnOkAlt    = $.cfn_setText(jsonData, "btnOk");			// Text : 선택완료
					doubleData.btnCloseAlt = $.cfn_setText(jsonData, "btnClose");		// Text : 닫기
					$.each($("input[id^=btnTrip]"), function(tripIdx) {
						if ($(this).hasClass('active')) {
							doubleData.travelType  = $(this).attr('value');
						}
					});
					doubleData.langFlag    = jsLang;
					doubleData.contextRoot = "/jejuair";
					doubleData.sectionNumber = index;
				//웹 접근성 20151123 Park
				if( $("#btnTripMulti").hasClass("active") ) {	//다구간일 경우
					doubleData.calTitle = $.cfn_setText(jsonData, "depDateCal2"); //두번째 구간 출발일 날짜 선택 달력
				}else{
					doubleData.calTitle = $.cfn_setText(jsonData, "retDateCal.title"); //도착일 날짜 선택 달력
				}


					// 출/도착지 레이어가 오픈 상태일 경우 레이어 닫고 달력 보이게
					var openId;

					$("button[name=ibtnOpen], div[name=ibtnOpen]").each(function(){
						openId = $("button[name=ibtnOpen], div[name=ibtnOpen]").attr("id");

						$("#"+openId).attr("name", "ibtnClose");
						$("#"+openId).removeClass("open");
						deleteLayer();
					});

					// GA TAG Event
					var jsAction = "";
					var jsLabel = "Des. Calendar";

					if ( $('#hidTripType').val() == "O" ){
						jsAction = "One Way";
					} else if ( $('#hidTripType').val() == "R" ){
						jsAction = "Round";
					} else if ( $('#hidTripType').val() == "M" ){
						jsAction = "Multi";
						jsLabel = "Route 2 &gt; Dep. Calendar";
					}

					//log("depDateCal ga -----&gt; Search Flight of Main, " + jsAction + ", " + jsLabel);
					//ga('send', 'event', "Search Flight of Main", jsAction, jsLabel);

					if($(this).attr("id") == "dateCal1"){
						ga('rollup.send', 'event', 'PC_RESERVATION', 'Booking_depdate', '');
					}else if($(this).attr("id") == "dateCal2"){
						ga('rollup.send', 'event', 'PC_RESERVATION', 'Booking_arrdate', '');
					}

					$.newFromToCal(doubleData);
					//[인터넷부킹고도화]
					var posY = $('.flightTab').offset().top - jQuery('#DoubleCalendar').outerHeight();
					jQuery("#DoubleCalendar").css({"top":posY});
				});

			});

			jsDepDate = $("#txtDate1").val().replace(/[^0-9]/gi,'');
			jsRetDate = $("#txtDate"+jsSelectSectionSize).val().replace(/[^0-9]/gi,'');

			// 여행계획의 날짜달력 선언
			var checkinDate = $.format(jsCurrentDate, dateFormatValue1);
			checkinDate = $.cfn_getDateFormat($.cfn_moveDate(checkinDate, 3, 'D'));
			$('#txtCheckInDate').val( checkinDate );
			$('#txtCheckInDate').data('initDate', $.cfn_moveDate(jsCurrentDate, 3, 'D'));
			$('#txtCheckInDate').data('initDate1', checkinDate);
			$('#txtCheckInDate').data('selectDate', $.cfn_moveDate(jsCurrentDate, 3, 'D'));
			$('#txtCheckInDate').data('selectDate1', checkinDate);
			$("#txtCheckInDate").data("restDate", "");
			$("#txtCheckInDate").data("restDate1", "");
			$('#txtCheckInDate').data('weekArea', "");
			$('#btnCheckInDate').click(function(){
			//&lt;![CDATA[
				$("#gWrap").append('&lt;div id="calandarSpace" &gt;&lt;/div&gt;&lt;div id="divCalandarSpace" &gt;&lt;/div&gt;');
			//]]&gt;
				singleData = new Object();
				singleData.initDate    = $('#txtCheckInDate').data('initDate1');
				singleData.fromId      = '#txtCheckInDate';
				singleData.target      = '#btnCheckInDate';
				singleData.callBack    = checkinCalComplete;
				singleData.dateFormat  = dateFormatValue1;
				singleData.isWeek      = false;
				singleData.chkWeekend  = false;
				singleData.title	   = "";
				singleData.btnOkAlt    = $.cfn_setText(jsonData, "btnOk");			// Text : 선택완료
				singleData.btnCloseAlt = $.cfn_setText(jsonData, "btnClose");		// Text : 닫기
				singleData.contextRoot = "/jejuair";

				$.singleCalendar(singleData);
			});

			var pickDate = $.format(jsCurrentDate, dateFormatValue1);
			pickDate = $.cfn_getDateFormat($.cfn_moveDate(pickDate, 3, 'D'));
			$('#txtPickDate').val( pickDate );
			$('#txtPickDate').data('initDate', $.cfn_moveDate(jsCurrentDate, 3, 'D'));
			$('#txtPickDate').data('initDate1', pickDate);
			$('#txtPickDate').data('selectDate', $.cfn_moveDate(jsCurrentDate, 3, 'D'));
			$('#txtPickDate').data('selectDate1', pickDate);
			$('#txtPickDate').data('restDate', "");
			$('#txtPickDate').data('restDate1', "" );
			$('#txtPickDate').data('weekArea', "" );

			$('#btnPickDate').click(function(){
			//&lt;![CDATA[
				$("#gWrap").append('&lt;div id="calandarSpace" &gt;&lt;/div&gt;&lt;div id="divCalandarSpace" &gt;&lt;/div&gt;');
			//]]&gt;
				doubleData = new Object();
				doubleData.initDate    = $('#txtPickDate').data('initDate1');
				doubleData.from        = '#txtPickDate';
				doubleData.to          = '#txtDropDate';
				doubleData.target      = '#btnPickDate';
				doubleData.callBack    = pickCalComplete;
				doubleData.dateFormat  = dateFormatValue1;
				doubleData.isWeek      = false;
				doubleData.chkWeekend  = true;
				doubleData.depWeekArea = $("#txtPickDate").data("weekArea");
				doubleData.retWeekArea = $("#txtDropDate").data("weekArea");
				doubleData.depHeader   = $.cfn_setText(jsonData, "pickDate");		// Text : 대여날짜
				doubleData.arrHeader   = $.cfn_setText(jsonData, "dropDate");		// Text : 반납날짜
				doubleData.btnOkAlt    = $.cfn_setText(jsonData, "btnOk");			// Text : 선택완료
				doubleData.btnCloseAlt = $.cfn_setText(jsonData, "btnClose");		// Text : 닫기
				doubleData.travelType  = "R";
				doubleData.langFlag    = jsLang;
				doubleData.contextRoot = "/jejuair";

				$.fromToCal(doubleData);

			});

			var dropDate = $.format(jsCurrentDate, dateFormatValue1);
			dropDate = $.cfn_getDateFormat($.cfn_moveDate(dropDate, 6, 'D'));
			$('#txtDropDate').val( dropDate );
			$('#txtDropDate').data('initDate', $.cfn_moveDate(jsCurrentDate, 6, 'D'));
			$('#txtDropDate').data('initDate1', dropDate);
			$('#txtDropDate').data('selectDate', $.cfn_moveDate(jsCurrentDate, 6, 'D'));
			$('#txtDropDate').data('selectDate1', dropDate);
			$('#txtDropDate').data('restDate', "");
			$('#txtDropDate').data('restDate1', "" );
			$('#txtDropDate').data('weekArea', "" );

			$('#btnDropDate').click(function(){
			//&lt;![CDATA[
				$("#gWrap").append('&lt;div id="calandarSpace" &gt;&lt;/div&gt;&lt;div id="divCalandarSpace" &gt;&lt;/div&gt;');
			//]]&gt;
				doubleData = new Object();
				doubleData.initDate    = $('#txtDropDate').data('initDate1');
				doubleData.from        = '#txtPickDate';
				doubleData.to          = '#txtDropDate';
				doubleData.target      = '#btnDropDate';
				doubleData.callBack    = dropCalComplete;
				doubleData.dateFormat  = dateFormatValue1;
				doubleData.isWeek      = false;
				doubleData.chkWeekend  = true;
				doubleData.depWeekArea = $("#txtPickDate").data("weekArea");
				doubleData.retWeekArea = $("#txtDropDate").data("weekArea");
				doubleData.depHeader   = $.cfn_setText(jsonData, "pickDate");		// Text : 대여날짜
				doubleData.arrHeader   = $.cfn_setText(jsonData, "dropDate");		// Text : 반납날짜
				doubleData.btnOkAlt    = $.cfn_setText(jsonData, "btnOk");			// Text : 선택완료
				doubleData.btnCloseAlt = $.cfn_setText(jsonData, "btnClose");		// Text : 닫기
				doubleData.travelType  = "R";
				doubleData.langFlag    = jsLang;
				doubleData.contextRoot = "/jejuair";

				$.fromToCal(doubleData);
			});

			function calComplete(sectionNumber) {
				sectionDate = $('#txtDate'+sectionNumber).data('selectDate');
				sectionDate1 = $('#txtDate'+sectionNumber).data('selectDate1');

				$('#txtDate'+sectionNumber).data('initDate',sectionDate);
				$('#txtDate'+sectionNumber).data('initDate1',sectionDate1);
				$('#txtDate'+sectionNumber).val(sectionDate1);

				for (var i = sectionNumber; i &gt; 1; i--) {

					thisSectionDate = $('#txtDate'+(i)).data('initDate');
					thisSectionDate1 = $('#txtDate'+(i)).data('initDate1');

					if($('#txtDate'+(i)).data('initDate')){
						if($('#txtDate'+(i - 1)).data('initDate')){

							prevSectionDate = $('#txtDate'+(i - 1)).data('initDate');
							prevSectionDate1 = $('#txtDate'+(i - 1)).data('initDate1');

							if(parseInt(prevSectionDate,10) &gt; parseInt(thisSectionDate,10)){
								$('#txtDate'+(i - 1)).val(thisSectionDate1);
								$('#txtDate'+(i - 1)).data('initDate',thisSectionDate);
								$('#txtDate'+(i - 1)).data('initDate1',thisSectionDate1);
								$('#txtDate'+(i - 1)).data('selectDate',thisSectionDate);
								$('#txtDate'+(i - 1)).data('selectDate1',thisSectionDate1);
							} else {
								$('#txtDate'+(i - 1)).val(prevSectionDate1);
								$('#txtDate'+(i - 1)).data('initDate',prevSectionDate);
								$('#txtDate'+(i - 1)).data('initDate1',prevSectionDate1);
							}
						}
					}
				}
				for (var i = sectionNumber; i &lt;= jsMaxSection; i++) {
					thisSectionDate = $('#txtDate'+(i)).data('initDate');
					thisSectionDate1 = $('#txtDate'+(i)).data('initDate1');

					if($('#txtDate'+(i + 1)).data('initDate')){

						nextSectionDate = $('#txtDate'+(i + 1)).data('initDate');
						nextSectionDate1 = $('#txtDate'+(i + 1)).data('initDate1');
						if(parseInt(thisSectionDate,10) &gt; parseInt(nextSectionDate,10)){
							$('#txtDate'+(i + 1)).val(thisSectionDate1);
							$('#txtDate'+(i + 1)).data('initDate',thisSectionDate);
							$('#txtDate'+(i + 1)).data('initDate1',thisSectionDate1);
							$('#txtDate'+(i + 1)).data('selectDate',thisSectionDate);
							$('#txtDate'+(i + 1)).data('selectDate1',thisSectionDate1);
						} else {

							$('#txtDate'+(i + 1)).val(nextSectionDate1);
							$('#txtDate'+(i + 1)).data('initDate',nextSectionDate);
							$('#txtDate'+(i + 1)).data('initDate1',nextSectionDate1);
						}
					}
				}

				jsDepDate = $("#txtDate1").val().replace(/[^0-9]/gi,'');
				jsRetDate = $("#txtDate"+jsSelectSectionSize).val().replace(/[^0-9]/gi,'');
				//[인터넷부킹고도화]
				if( $("#hidTripType").val()  == "M"){
					var nextSectionNo = sectionNumber+1;
					if($("#txtDepStn"+nextSectionNo).is(':visible')){
						$("#txtDepStn"+nextSectionNo).trigger("click");
					}
				}
			}

			function checkinCalComplete(){
				checkinDate = $('#txtCheckInDate').data('selectDate');
				checkinDate1 = $('#txtCheckInDate').data('selectDate1');

				$('#txtCheckInDate').val(checkinDate1);
				$('#txtCheckInDate').data('initDate',checkinDate);
				$('#txtCheckInDate').data('initDate1',checkinDate1);
			}

			function pickCalComplete(){
				pickDate = $('#txtPickDate').data('selectDate');
				pickDate1 = $('#txtPickDate').data('selectDate1');

				$('#txtPickDate').data('initDate',pickDate);
				$('#txtPickDate').data('initDate1',pickDate1);
				$('#txtPickDate').val(pickDate1);

				if($('#txtDropDate').data('initDate')){
					dropDate = $('#txtDropDate').data('initDate');
					dropDate1 = $('#txtDropDate').data('initDate1');

					if(parseInt(pickDate,10) &gt; parseInt(dropDate,10)){
						$('#txtDropDate').val(pickDate1);
						$('#txtDropDate').data('initDate',pickDate);
						$('#txtDropDate').data('initDate1',pickDate1);
						$('#txtDropDate').data('selectDate',pickDate);
						$('#txtDropDate').data('selectDate1',pickDate1);

		                $("#doYear").val(pickDate.substring(0,4));
		                $("#doMonth").val(pickDate.substring(4,6));
		                $("#doDay").val(pickDate.substring(6,8));
					} else {
						$('#txtDropDate').val(dropDate1);
						$('#txtDropDate').data('initDate',dropDate);
						$('#txtDropDate').data('initDate1',dropDate1);

		                $("#doYear").val(dropDate.substring(0,4));
		                $("#doMonth").val(dropDate.substring(4,6));
		                $("#doDay").val(dropDate.substring(6,8));
					}

				} else {
					$('#txtDropDate').data('initDate',pickDate);
					$('#txtDropDate').data('initDate1',pickDate1);
				}

                $("#puYear").val(pickDate.substring(0,4));
                $("#puMonth").val(pickDate.substring(4,6));
                $("#puDay").val(pickDate.substring(6,8));
			}

			function dropCalComplete() {
				dropDate = $('#txtDropDate').data('selectDate');
				dropDate1 = $('#txtDropDate').data('selectDate1');

				$('#txtDropDate').data('initDate',dropDate);
				$('#txtDropDate').data('initDate1',dropDate1);
				$('#txtDropDate').val(dropDate1);

				if($('#txtPickDate').data('initDate')){
					pickDate = $('#txtPickDate').data('initDate');
					pickDate1 = $('#txtPickDate').data('initDate1');

					if(parseInt(pickDate,10) &gt; parseInt(dropDate,10)){
						$('#txtPickDate').val(dropDate1);
						$('#txtPickDate').data('initDate',dropDate);
						$('#txtPickDate').data('initDate1',dropDate1);
						$('#txtPickDate').data('selectDate',dropDate);
						$('#txtPickDate').data('selectDate1',dropDate1);

		                $("#puYear").val(dropDate.substring(0,4));
		                $("#puMonth").val(dropDate.substring(4,6));
		                $("#puDay").val(dropDate.substring(6,8));
					} else {
						$('#txtPickDate').val(pickDate1);
						$('#txtPickDate').data('initDate',pickDate);
						$('#txtPickDate').data('initDate1',pickDate1);

		                $("#puYear").val(pickDate.substring(0,4));
		                $("#puMonth").val(pickDate.substring(4,6));
		                $("#puDay").val(pickDate.substring(6,8));
					}
				}

                $("#doYear").val(dropDate.substring(0,4));
                $("#doMonth").val(dropDate.substring(4,6));
                $("#doDay").val(dropDate.substring(6,8));
			}
	 	}

		//--------------------------------------
	 	// 조회조건 인원 초기 상태
	 	//--------------------------------------
		function fn_personsInit(){

			// selectBox 성인1, 소아0, 유아0으로 세팅
			$('#selADT').attr('data', '1');
			$('#selCHD').attr('data', '0');
			$('#selINF').attr('data', '0');

			var jsAdt = "";
			var jsChd = "";
			var jsInf = "";

			if(jsLang != "RU") {
				jsAdt = $.cfn_setText(jsonData, "lblAdt")+" 1"+$.cfn_setText(jsonData, "lblPerson");
				jsChd = $.cfn_setText(jsonData, "lblChd")+" 0"+$.cfn_setText(jsonData, "lblPerson");
				jsInf = $.cfn_setText(jsonData, "lblInf")+" 0"+$.cfn_setText(jsonData, "lblPerson");
			} else {
				jsAdt = $.cfn_setText(jsonData, "lblAdt");
				jsChd = $.cfn_setText(jsonData, "lblChd");
				jsInf = $.cfn_setText(jsonData, "lblInf");
			}

			$('#selADT').attr("value", jsAdt);
			$('#selCHD').attr("value", jsChd);
			$('#selINF').attr("value", jsInf);
			//$('#selADT').attr('value', '성인 1명');
			//$('#selCHD').attr('value', '소아 0명');
			//$('#selINF').attr('value', '유아 0명');

			$('#strADT').text($('#selADT').attr('data'));
			$('#strCHD').text($('#selCHD').attr('data'));
			$('#strINF').text($('#selINF').attr('data'));

		}	// fn_personsInit

		//--------------------------------------
	 	// 인원 SelectBox 언어에 따라 구성
	 	//--------------------------------------
		function fn_createSelectBox(){
			var htmlADT = "";
			var htmlCHD = "";
			var htmlINF = "";

			$("#ulADT").empty();
			$("#ulCHD").empty();
			$("#ulINF").empty();

			var textADT = "";
			var textCHD = "";
			var textINF = "";

			if(jsLang != "RU") {
				textADT = $.cfn_setText(jsonData, "lblAdt");
				textCHD = $.cfn_setText(jsonData, "lblChd");
				textINF = $.cfn_setText(jsonData, "lblInf");
			}

			for (var i = 0; i &lt; 10; i++) {

				if ( i != 0 ) {
					htmlADT +=	'	&lt;li onclick=\'javascript:confirmSelectedValue("selADT", this, "", true);\'&gt;'+
								'		&lt;button type="button"&gt;'+
								'			&lt;span data="'+i+'"&gt;'+textADT+' '+i+$.cfn_setText(jsonData, "lblPerson")+'&lt;/span&gt;'+
								'		&lt;/button&gt;'+
								'	&lt;/li&gt;';
				}

				htmlCHD +=		'	&lt;li onclick=\'javascript:selectValue("selCHD", this);\'&gt;'+
								'		&lt;button type="button"&gt;'+
								'			&lt;span data="'+i+'"&gt;'+textCHD+' '+i+$.cfn_setText(jsonData, "lblPerson")+'&lt;/span&gt;'+
								'		&lt;/button&gt;'+
								'	&lt;/li&gt;';


				htmlINF +=		'	&lt;li onclick=\'javascript:confirmSelectedValue("selINF", this, "", true);\'&gt;'+
								'		&lt;button type="button"&gt;'+
								'			&lt;span data="'+i+'"&gt;'+textINF+' '+i+$.cfn_setText(jsonData, "lblPerson")+'&lt;/span&gt;'+
								'		&lt;/button&gt;'+
								'	&lt;/li&gt;';
			}

			$("#ulADT").html(htmlADT);
			$("#ulCHD").html(htmlCHD);
			$("#ulINF").html(htmlINF);

		}	// fn_createSelectBox


		//--------------------------------------
	 	// GA TAG Event - 탑승객 인원
	 	//--------------------------------------
		function fn_gaEventPax(label) {

				// GA TAG Event
				var jsAction = "";

			if ( $('#hidTripType').val() == "O" ){
				jsAction = "One Way";
			} else if ( $('#hidTripType').val() == "R" ){
				jsAction = "Round";
			} else if ( $('#hidTripType').val() == "M" ){
				jsAction = "Multi";
			}

			//log("fn_gaEventPax ga -----&gt; Search Flight of Main, " + jsAction + ", " + label);
			//ga('send', 'event', "Search Flight of Main", jsAction, label);

		}

		function post_to_url(path, params) {
			var frm = document.createElement("form");
			frm.setAttribute("method","post");
			frm.setAttribute("action", path);

			for (var key in params) {
				alert(params[key]);
				var hiddenField = document.createElement("input");
				hiddenField.setAttribute("type", "hidden");
				hiddenField.setAttribute("name", key);
				hiddenField.setAttribute("value", params[key]);
				frm.appendChild(hiddenField);
			}
			document.body.appendChild(frm);
			frm.submit();
		}

		cfn_getDateSubtraction = function(toDate, fromDate){
			var day = 1000*60*60*24;
			toDate = $.cfn_getDateFormat(toDate,'yyyy-MM-dd');
			fromDate = $.cfn_getDateFormat(fromDate,'yyyy-MM-dd');
			var toDt 	= toDate.split('-');
			var fromDt 	= fromDate.split('-');
			toDate = new Date(toDt[0],toDt[1],toDt[2]);
			formDate = new Date(fromDt[0],fromDt[1],fromDt[2]);
			return parseInt((formDate - toDate)/day);
		};

		//--------------------------------------
	 	// 예약하기 버튼
	 	//--------------------------------------
	 	function fn_doReservation(){
	 		$('#btnReservation').click(function(evt){
				// GA TAG Event
	 			var jsAction = "";
	 			var allianceArrCode = $('#allianceArrCode').val();
				// add by dark
				if ( jsStnId.indexOf("ArrStn") != -1 &amp;&amp;  allianceArrCode != '' ) {


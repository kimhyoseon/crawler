$(document).ready(function() {
	common_gnb.init_loading();

	jQuery(".loginOpen").on("click",function(){
		jQuery(".loginWrap").show();
		jQuery("#gnb_Id1").focus();
		jQuery("body").append("&lt;div class='blackLightbox'&gt;&lt;/div&gt;");
		//[인터넷부킹고도화]20160520 상단 배너 있을때 딤 영역 top 값 변경
		if(jQuery('.pageTopBanner').is(':visible')){
			jQuery('.blackLightbox').css({
				'top' : $('.pageTopBanner').height() + 34
			})
		}
		var layer = $tgt || "";
		if ( layer != "" ) {
			deleteLayer();
		}

		// 비회원 로그인 약관 로딩 변경
		var collectionPersonal = $("#p05_collection_personal");
		if (collectionPersonal.text().length &lt;= 0) {
			collectionPersonal.load("/jejuair/etc/html/p05_collection_personal_"+ jsLang +".html", function() {});
		}

	});
	jQuery(".loginClose button").add(".mcloseBtn").on("click",function(){
		jQuery(".loginWrap").hide();
		jQuery(".blackLightbox").remove();
	});

	//비회원로그인 탭
	 jQuery(".saletoemailOpen").on("click",function(){
	    jQuery(".saletoemaiWrap").show();
	    jQuery("body").append("&lt;div class='blackLightbox'&gt;&lt;/div&gt;");
		//[인터넷부킹고도화]20160520 상단 배너 있을때 딤 영역 top 값 변경
		if(jQuery('.pageTopBanner').is(':visible')){
			jQuery('.blackLightbox').css({
				'top' : $('.pageTopBanner').height() + 34
			})
		}
	    var layer = $tgt || "";
	    if ( layer != "" ) {
	      deleteLayer();
	    }
	  });
	  jQuery(".loginClose button").add(".saletoemaicloseBtn").on("click",function(){
	    jQuery(".saletoemaiWrap").hide();
	    jQuery(".blackLightbox").remove();
	  });


	//!--- 언어선택 ---
	$("button.selectLenBtn").click(function(e) {
		e.preventDefault();
		$("div.selectLen").toggleClass("on");
	});
	// 클릭 레이어는 포커스 아웃 시 자동으로 히든되지 않도록
	/*
	$(".selectLen").on("mouseleave",function(){
		$(".selectLen").removeClass("on");
	});
	*/
	// click remove
	$('.selectLenUl &gt; li.current').on('click', function() {
		$(".selectLen").removeClass("on");
	});
	//웹접근성(Tocanz) - 20151123 - Kang
	$("button.selectLenBtn").focusin(function(){
		$("div.selectLen").toggleClass("on");
	});
	$("#txtGoogleSearch").focusin(function(){
		$(".selectLen").removeClass("on");
	});
	$('#gnb_email_01').focusin(function(){
		$(".selectLen").removeClass("on");
	});
	$('#gnb_logout').focusin(function(){
		$(".selectLen").removeClass("on");
	});


	//[인터넷부킹고도화]
	$("div.selectLen &gt; ul &gt; li:not('.current')").on('click',function(e) {
		e.preventDefault();

		$(this).siblings().removeClass("current");
		$(this).addClass("current");
		$("button.selectLenBtn")
		    .find("span").text( $(this).find("a").text() ).end()
		    .find("input:hidden").val();
		$("div.selectLen").toggleClass("on");

		var languageCountry = $(this).find("a").attr("id");
		var languageText    = $(this).find("a").text();
		var languageType    = "";
		var lnaguageUrl = "";

		if ( "KOR" == languageCountry ) {
			languageType = "KR";
			lnaguageUrl = "kr";
		} else if ( "JPN" == languageCountry ) {
			languageType = "JP";
			lnaguageUrl = "jp";
		} else if ( "CHN" == languageCountry ) {
			languageType = "CN";
			lnaguageUrl = "cn";

//홍콩,태국 추가 (2015.01.15 네티브)
		} else if ( "HONG" == languageCountry ) {
			languageType = "HK";
			lnaguageUrl = "ho";
		} else if ( "THAI" == languageCountry ) {
			languageType = "TH";
			lnaguageUrl = "th";
//홍콩,태국 추가 (2015.01.15 네티브)

//홍콩,태국 추가 (2015.03.04 네티브)
		} else if ( "TWN" == languageCountry ) {
			languageType = "TW";
			lnaguageUrl = "tw";
		} else if ( "VNM" == languageCountry ) { // 베트남 추가. 2015.10.23 FFP시스템개선
			languageType = "VN";
			lnaguageUrl = "vn";
		} else if("MFM" == languageCountry){ // 마카오 추가. 2016.09.28
			languageType = "HK";
			lnaguageUrl = "mf";
		} else if("PHIL" == languageCountry){
			languageType = "EN";
			lnaguageUrl = "ph";
		} else if("ENG" == languageCountry){
			languageType = "EN";
			lnaguageUrl = "en";
		} else if("MYS" == languageCountry){
			languageType = "EN";
			lnaguageUrl = "my";
		} else if("RUS" == languageCountry){
			languageType = "RU";
			lnaguageUrl = "ru";
		} else if("LAOS" == languageCountry){
			languageType = "EN";
			lnaguageUrl = "la";
		} else {
			languageType = "EN";
			lnaguageUrl = "en";
		}
/*
		var currentUrl = location.href
		currentUrl = currentUrl.indexOf("dark");

		var post_data = { "LANGUAGE_COUNTRY" : languageCountry, "LANGUAGE_TYPE" : languageType };
		$.post("/jejuair/common/language_session.jsp", post_data, function(){

			if(currentUrl &gt; -1){
				var tgURL = jsReturnHost +"/jejuair/dark/main.jsp";
			} else {
				//document.location.href = "/jejuair/main.jsp";
				var tgURL="";

				// 20160722 current url check
				var referrer = location.href;

				if(referrer){
					tgURL = referrer;
				}else{
					tgURL = jsReturnHost +"/jejuair/main.jsp";
				}
			}

			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		});

*/
		var currentUrl = location.href
		currentUrl = currentUrl.indexOf("dark");

		var post_data = { "LANGUAGE_COUNTRY" : languageCountry, "LANGUAGE_TYPE" : languageType };
		$.post("/jejuair/common/language_session.jsp", post_data, function(){

			var tgURL="";

			if(currentUrl &gt; -1){
				var tgURL = jsReturnHost +"/jejuair/dark/main.jsp";
			} else {
				//document.location.href = "/jejuair/main.jsp";

				// 20160722 current url check
				var referrer = location.href;

				// 20170419 다국어, SEO 개선
				var urlIndex = referrer.indexOf('/jejuair/');
				var selectLang = referrer.substr(referrer.indexOf("/jejuair/")+8,4);

				var languageTypeLowerCase = languageType.charAt(0).toLowerCase() + languageType.charAt(1).toLowerCase();
				if(referrer){
					if(selectLang=='/kr/' || selectLang=='/jp/' || selectLang=='/en/' || selectLang=='/cn/' || selectLang=='/th/' || tmplang == '/ph/'
						|| selectLang=='/ho/' || selectLang=='/mf/' || selectLang=='/tw/' || selectLang=='/hk/' || selectLang=='/vn/' || selectLang=='/my/' || selectLang=='/ru/' || selectLang=='/la/'){
//						tgURL = "/jejuair/" + languageTypeLowerCase + referrer.substr(urlIndex+11, referrer.length);
						tgURL = "/jejuair/" + lnaguageUrl + referrer.substr(urlIndex+11, referrer.length);
					}else{
						tgURL = referrer;
					}
//					tgURL = referrer;

				}else{
//					tgURL = jsReturnHost +"/jejuair/main.jsp";
					tgURL = jsReturnHost +"/jejuair/kr/main.do";
				}
			}

			jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
		});


	});

	/**
	 * @Description : [인터넷부킹고도화] 상단의 다국어 메뉴 클릭시 뜨는 레이어의 현재나라를 제일 위로 올림.
	 */
	(function() {

		var listNode = $('div.selectLen &gt; ul &gt; li'),
			currentNode = listNode.filter('.current');

		if(listNode.eq(0).get(0) === currentNode.get(0)) return;
		listNode.eq(0).before(currentNode);

	})();

	//!--- 여행사 우대서비스 선택 ---
	$("button.selectCompanyBtn").click(function(e) {
		e.preventDefault();
		jQuery(".selectCompanyMenu").toggleClass("on");
		return false;
	});
	/*
	$(".selectCompanyMenu").on("mouseleave",function(){
		jQuery(".selectCompanyMenu").removeClass("on");
	});
	*/

	$("div.selectCompanyMenu &gt; ul &gt; li").click(function(e) {
		e.preventDefault();
		e.stopPropagation();

		var site_id =  $(this).attr("id") || "";
		var cursor_style = $(this).find("a").css("cursor") || "default";

		if ("" == site_id) {
            return;
        } else if ("default" == cursor_style) {
            return;
        }
		//document.location.href = common_gnb.gnb_map["companyMenu"][site_id];

		window.open(common_gnb.gnb_map["companyMenu"][site_id], "_blank");
	});
	//!--- 기업 우대서비스 선택 ---
	$("button.selectCompanyBenefitBtn").click(function(e) {
		window.open(jsReturnHost+common_gnb.gnb_map["companyBenefitMenu"]["company_join"], "_self");
	});

	//!--- 로그인 요청 start !!! ---
	$("input[name='gnb_pid'], input[name='gnb_emailDomain']").keydown(function(e) {
		var that_name = $(this).attr("name");
		if(e.keyCode == 13) {
			if ( "gnb_pid" == that_name ) {
				$("#gnb_idLogin").trigger("click");
			} else if ( "gnb_emailDomain" == that_name ) {
				$("#gnb_noLogin1").trigger("click");
			}
		}
	});

	$("#gnb_idLogin, #gnb_noLogin1").click(function(){
		//아이디로 로그인
		if(this.id == 'gnb_idLogin'){
			common_gnb.idLoginChk();
		//비회원 로그인
		}else if(this.id == 'gnb_noLogin1'){
			common_gnb.noLoginLoginChk();
		//비회원 로그인	예약정보
		}else if(this.id == 'gnb_noLogin2'){
			if(menu == "mypage"){
				common_gnb.noLoginReservChk();
			}else{
				common_gnb.noLoginReservChk();
			}
		}
		/*if ( eval("typeof fn_setSession") == "function" ) {
			clearTimeout(common_gnb.login_timer);
			common_gnb.login_timer = setTimeout(fn_setSession, 2000);
		}*/
		if ( eval("typeof cfn_clickEvent") == "function" ) {
			var userCode = "";
			if ( this.id == 'gnb_idLogin' ) {
				userCode = $("input[name='gnb_userid']").val();
			} else {
				userCode = $.trim($("input[name='gnb_emailID']").val()) + "@" + $.trim($("input[name='gnb_emailDomain']").val());
			}
			cfn_clickEvent(userCode, '', '', 'Login', 'Main', 'IBE', '');
		}
	});
	//!--- 로그인 요청 period !!! ---

	$("a[name='gnb_memCheck']").click(function(e) {
		e.preventDefault();
		//document.location.href = "/jejuair/login/join/member_check.jsp";
		//회원가입 간소화 링크로 변경 20161124
		//var tgURL = jsReturnHost +"/jejuair/login/join/member_check.jsp";
		var tgURL = jsReturnHost +"/jejuair/kr/login/join/easyJoinMain.do";
		jQuery('&lt;form action="'+tgURL+'" method="post" target="_self"&gt;&lt;/form&gt;').appendTo('body').submit().remove();
	});


					e.preventDefault();
					setCookie("openSubCookie","done",1);
					$('.mainBandBanner').slideUp(200);
				});

				//뉴스 더보기 클릭시 펼침
				$("#btnMoreNews").on("click",function(e){
					e.preventDefault();

					var btnMore = $(this);
					var newsList = $(".mnewsList .newslist");

					if(!btnMore.hasClass('on')){
						newsList.animate({
							'height' : 130
						},150,function(){
							btnMore.addClass('on');
						});
					}
					else{
						newsList.animate({
							'height' : 22
						},150,function(){
							btnMore.removeClass('on');
						});
					}
				});

			}


				}
				$(".newPointMenu2").toggle();
			});
			$(".pointMenu").mouseleave(function(){
				$(".btnList").toggleClass("active");
				if($(".btnList").hasClass("active")){  //웹접근성 20151209 Park
					$(".btnList").html($.cfn_setText(jsonTopData, "closeMyPointInfo"));
				}else{
					$(".btnList").html($.cfn_setText(jsonTopData, "openMyPointInfo"));

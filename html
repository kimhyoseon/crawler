                    var oHidTripType    = $('#hidTripType').val(); // TRIPTYPE
					var oLangCode       = "";

					//fnAbbMsgPopCall(oDepStn, oAllianceArrStn, oTxtDate1, oTxtDate2, oSelADT, oSelCHD, oSelINF,oHidTripType);


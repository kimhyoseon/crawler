							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=ja-jp";
						} else if( oLangCode == "TH" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=th-th";
						} else if( oLangCode == "VN" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=vi-vn";
						} else if( oLangCode == "CN" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=zh-cn";
						} else if( oLangCode == "TW" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=zh-tw";
						}else if( oLangCode == "RU" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=ru-ru";
						} else {
							location.href = "https://valuealliance.jejuair.net/#/start?type=oneway"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=en-us";
						}
					} else if ( oHidTripType == "R" ){
						if( oLangCode == "KR" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=ko-kr";
						} else if( oLangCode == "JP" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=ja-jp";
						} else if( oLangCode == "TH" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=th-th";
						} else if( oLangCode == "VN" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=vi-vn";
						} else if( oLangCode == "CN" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=zh-cn";
						} else if( oLangCode == "TW" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=zh-tw";
						} else if( oLangCode == "RU" ) {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=ru-ru";
						} else {
							location.href = "https://valuealliance.jejuair.net/#/start?type=return"+"&amp;origin="+oDepStn+"&amp;destination="+oAllianceArrStn+"&amp;departureDate="+oTxtDate1+"&amp;returnDate="+oTxtDate2+"&amp;paxAdult="+oSelADT+"&amp;paxChild="+oSelCHD+"&amp;paxInfant="+oSelINF+"&amp;locale=en-us";
						}
					}
					return false;
				}
				// end


				if ( $('#hidTripType').val() == "O" ){
					jsAction = "One Way";
	 			} else if ( $('#hidTripType').val() == "R" ){
	 				jsAction = "Round";
	 			} else if ( $('#hidTripType').val() == "M" ){
	 				jsAction = "Multi";
	 			}

				//[인터넷부킹고도화]
				$('#hidPayType').val($('input[name="ra"].active').attr('value'));

				//log("btnReservation ga -----&gt; Search Flight of Main, " + jsAction + ", Reservation Processing Button" );
				//ga('send', 'event', "Search Flight of Main", jsAction, "Reservation Processing Button");

				// 정산 점검 시간 체크. 2014.06.03 LJY
		 		if(!cfn_checkAdjustmentTerminate(comJsonData, "235458", "235500", "000500", "msgReservationTimeOut")) return false;

				// 시스템 점검 시간 체크. 2014.06.18 LJY
				if(jsRouteType == "DOM"){
					if(!cfn_checkSystemAdjustment(comJsonData, '20180120235458', '20160828030000 ', '20160828040000 ', 'msgNoticeTime')) return false;
				}else{
					if(!cfn_checkSystemAdjustment(comJsonData, '20180120235458', '20160828030000 ', '20160828040000 ', 'msgNoticeTime')) return false;
				}

				// addFlag가 "Y"이면 가져온 메시지 alert 2014.06.03
				if ( jsAddFlag == "Y" ){
					alert(jsDescDetail);

					return;
				}

				// 유효성체크하자
				if ($('#hidTripType').val() == "R"){
					if($("#hidDepStn1").val() == ""){
			 			alert($.cfn_setText(jsonData, "msgDepStn"));		// "출발지를 선택해주세요."
			 			$('#btnDepStn1').focus(); //웹접근성(Tocanz) - 20151202 - Kang
			 			return;
			 		} else if($("#hidArrStn2").val() == ""){
			 			alert($.cfn_setText(jsonData, "msgArrStn"));		// "도착지를 선택해주세요."
			 			$('#btnArrStn2').focus(); //웹접근성(Tocanz) - 20151202 - Kang
			 			return;
			 		}
				} else if ($('#hidTripType').val() == "M"){

					if(jsRouteType == 'DOM'){
						// "다구간 조회 시 국내선만으로 조회하실 수 없습니다."
						alert($.cfn_setText(jsonData, "msgNotOnlyDomestic"));
						return ;
					}

					for(var i = 1; i &lt;= jsSelectSectionSize; i++){
	 					if ( $("#hidDepStn" + i).val() == "" || $("#hidDepStn" + i).val() == $.cfn_setText(jsonData, "lblDep") ){
	 						alert($.cfn_setText(jsonData, "msgDepStn"));
							$("#btnDepStn" + i).focus();
		 		 			return;
	 					} else if( $("#hidArrStn" + i).val() == "" || $("#hidArrStn" + i).val() == $.cfn_setText(jsonData, "lblArr") ){

		 		 			// "도착지를 선택해주세요."
		 		 			alert($.cfn_setText(jsonData, "msgArrStn"));
							$("#btnArrStn" + i).focus();
		 		 			return;
		 		 		}
	 				}
				} else if ($('#hidTripType').val() == "O") {
					if($("#hidDepStn1").val() == ""){
			 			alert($.cfn_setText(jsonData, "msgDepStn"));		// "출발지를 선택해주세요."
			 			$('#btnDepStn1').focus();//웹접근성(Tocanz) - 20151202 - Kang
			 			return;
			 		} else if($("#hidArrStn2").val() == ""){
			 			alert($.cfn_setText(jsonData, "msgArrStn"));		// "도착지를 선택해주세요."
			 			$('#btnArrStn2').focus();//웹접근성(Tocanz) - 20151202 - Kang
			 			return;
			 		}
				}


				var paxCnt = parseInt($('#selADT').attr('data')) + parseInt($('#selCHD').attr('data'));	// 성인+소아
				// 성인 + 소아 인원수가 9 명을 초과할 수 없음
				if(paxCnt &gt; 9){
					//"인터넷 예약은 1회 9명까지 가능합니다.\n10명 이상은 단체 항공권 문의로 부탁드립니다.";
					alert($.cfn_setText(jsonData, "msgPaxCnt"));

					fn_personsInit();		// 초기화 : 인원 selectBox 성인1, 소아0, 유아0으로 세팅

					return;
				}

				if( $('#hidDualFlag').val() == "Y" &amp;&amp; $('#hidPayType').val() == "POINT"){
					alert($.cfn_setText(jsonData, "msgNotUsePoint"));
					return false;
				}

				if(jsMultiFlag == "Y"){
					//첫구간 출발지와 두번째 도착지가 같은경우 왕복운임과 같기때문에 왕복로직타게함
					if ($('#hidDepStn1').val() != "" &amp;&amp; $('#hidDepStn1').val() == $('#hidArrStn2').val()
							&amp;&amp; $('#hidDepStn2').val() == $('#hidArrStn1').val()) {
						//jsMultiFlag = "";

						jsArrayResDep = [];
						jsArrayResRet = [];

						$('#hidTripType').val("R");

						jsSelectSectionSize = 2;

					}
				}
				var jsReqDataObj = new Object();

				jsReqDataObj.mainFlag		= $('#hidMain').val();
				jsReqDataObj.routeType		= $('#hidRouteType').val();
				jsReqDataObj.tripType		= $('#hidTripType').val();
				jsReqDataObj.payType		= $('#hidPayType').val();

				if (jsMultiFlag != "Y") {
					jsReqDataObj.depStn1		= $('#hidDepStn1').val() || "";
					jsReqDataObj.arrStn1		= $('#hidArrStn1').val() || "";
					jsReqDataObj.depStn2		= $('#hidDepStn2').val() || "";
					jsReqDataObj.arrStn2		= $('#hidArrStn2').val() || "";
					jsReqDataObj.depStnName1	= $('#txtDepStn1').val();
					jsReqDataObj.arrStnName1	= $('#txtArrStn1').val();
					jsReqDataObj.depStnName2	= $('#txtDepStn2').val();
					jsReqDataObj.arrStnName2	= $('#txtArrStn2').val();
					jsReqDataObj.depDate1		= $('#txtDate1').val();	// YYYYMMDD
					jsReqDataObj.depDate2		= $('#txtDate2').val();	// YYYYMMDD
					jsReqDataObj.country1		= $('#hidCountry1').val() || "";
					jsReqDataObj.country2		= $('#hidCountry2').val() || "";
				} else {
					for(var i = 1; i &lt;= jsSelectSectionSize; i++){
						jsReqDataObj['depStn'+i]	= $('#hidDepStn' + i).val() || "";
						jsReqDataObj['arrStn'+i]	= $('#hidArrStn' + i).val() || "";

						jsReqDataObj['depStnName'+i]	= $('#txtDepStn' + i).val();
						jsReqDataObj['arrStnName'+i]	= $("#txtArrStn" + i).val();

						jsReqDataObj['depDate'+i]		= $('#txtDate' + i).val();	// YYYYMMDD
						jsReqDataObj['country'+i]		= $('#hidCountry' + i).val() || "";
					}
					jsReqDataObj.country3		= $('#hidCountry3').val() || "";
					jsReqDataObj.country4		= $('#hidCountry4').val() || "";
				}

				jsReqDataObj.selectSectionSize		= jsSelectSectionSize;
				jsReqDataObj.adtPaxCnt		= $('#selADT').attr('data');
				jsReqDataObj.chdPaxCnt		= $('#selCHD').attr('data');
				jsReqDataObj.infPaxCnt		= $('#selINF').attr('data');


				jsReqDataObj.dualFlag		= $('#hidDualFlag').val() || "";
				jsReqDataObj.multiFlag		= jsMultiFlag;

				//예약하기 버튼 클릭 시 Google Analytics설정
				var label = "";

				if($("#btnTripReturn").prop("checked")){
					label = "왕복_";
				} else if ($("#btnTripSingle").prop("checked")){
					label = "편도_";
				} else if ($("#btnTripMulti").prop("checked")){
					label = "다구간_";
				} else {
					label = "단체예약_";
				}

				label += $("#txtDepStn1").val() +"_" + $("#txtArrStn2").val() +"_"+ $("#selADT").val()  +"_"+ $("#selCHD").val() +"_"+ $("#selINF").val();
				onclick ="ga('rollup.send', 'event', 'PC_MAIN', 'Booking_Search', label); n_click_logging('https://www.jejuair.net/jejuair/userclick.jsp?jcategory=PC_MAIN&amp;jaction=Booking_Search&amp;jlabel=Reservation',location.href);";
				// Google Analytics설정 end

				document.searchForm.method = "post";
				document.searchForm.hidRequestData.value = JSON.stringify(jsReqDataObj);
				document.searchForm.action = jsReturnHost + "/jejuair/kr/com/jeju/ibe/goAvail.do";

				//netthru start
				try{
					var n_p22 = cfn_getDateSubtraction($('#txtDate1').val().replace(/-/gi, ""), $('#txtDate2').val().replace(/-/gi, ""));
					var n_toDay = $.datepicker.formatDate('yymmdd',new Date());
            		var n_p23 = cfn_getDateSubtraction(n_toDay, $('#txtDate1').val().replace(/-/gi, ""));
            		var ticketType = "";
            		var chkPoint = "";

            		if($('#hidPayType').val() == "POINT") {
            			chkPoint = "포인트결제"
            		} else {
            			chkPoint = "일반결제"
            		}
            		if($("#btnTripReturn").prop("checked")){
            			ticketType = "왕복";
    				} else if ($("#btnTripSingle").prop("checked")){
    					ticketType = "편도";
    				} else if ($("#btnTripMulti").prop("checked")){
    					ticketType = "다구간";
    				}

            		var loginChk1 = "";
            		var loginChk2 = "";

	            	var netthru = new Object();
	            	netthru._n_p1 = "KR";
	            	netthru._n_p3 = "IBE";
					if(loginChk1 != ""){
						if(loginChk2 != ""){
							netthru._n_p4 = "ID";
						} else {
							netthru._n_p4 = "email";
						}
					} else {
						netthru._n_p4 = "none";
					}
					netthru._n_p16 = $('#selADT').attr('data');
					netthru._n_p17 = $('#selCHD').attr('data');
					netthru._n_p18 = $('#selINF').attr('data');
					netthru._n_p20 = ticketType;
					netthru._n_p22 = n_p22;
					netthru._n_p23 = n_p23;
					netthru._n_p24 = $('#txtDate1').val().replace(/-/gi,'');
					netthru._n_p25 = $('#txtDate2').val().replace(/-/gi,'');
					netthru._n_p28 = chkPoint;

					if ($("#btnTripMulti").prop("checked")){
						netthru._n_p44 = $("#hidDepStn1").val();
						netthru._n_p45 = $("#hidArrStn1").val();
						netthru._n_p46 = $("#hidDepStn2").val();
						netthru._n_p47 = $("#hidArrStn2").val();
					} else {
						netthru._n_p44 = $("#hidDepStn1").val();
						netthru._n_p45 = $("#hidArrStn2").val();
					}

					netthru._n_p48 = $('#txtDepStn1').val();
					netthru._n_p49 = $('#txtArrStn2').val();;
					netthru._n_p50 = "/jejuair/KR/main.do";

					action_logging(netthru);
				}catch(exception) {}
				//netthru end

				document.searchForm.submit();

			});	// $('#btnReservation').click
		}


	 	//--------------------------------------
	 	// 성인보다 유아가 많은 경우
	 	//--------------------------------------
		function fn_infantPaxCntCheck(){
			var adultPaxCnt  = $('#selADT').attr('data');
			var infantPaxCnt = $('#selINF').attr('data');

			if (infantPaxCnt &gt; adultPaxCnt) {
				// "성인1명에 유아1명만 예약하실 수 있으며, 나머지 유아는 소아로 예약하셔야 합니다."
				alert($.cfn_setText(jsonData, "msgInfantCnt"));
				optionSelectByValue("selINF", adultPaxCnt, false);
			}

		}
		//--------------------------------------
	 	// 출/도착지 구성
		// alliancecode 추가 20170321 by dark
	 	//--------------------------------------
		function fn_setAirport(id, airName, airCode, cityCode, country, routeType, addFlag, descDetail, connectionFlag, allianceCode){
			// add by dark
			jsAllianceCode = allianceCode;
			$('#allianceArrCode').attr('value', allianceCode);
			// end

			if($("#hidTripType").val() == "M"){

				if (id.indexOf("ArrStn") != -1 &amp;&amp; $('#hidFirstDepStnCity').val() == cityCode) {

					if (id.substring(9) % 2 != 0) {
						alert($.cfn_setText(jsonData, "msgCanNotThirdArr"));
						return false;
					} else {

						jsSelectSectionSize = id.substring(9);
						var prevSegDate = $('#txtDate' + jsSelectSectionSize).val();

						$("div[id^=divRoute]").each(function(idx){
							if($(this).css("display") == "block" &amp;&amp; idx &gt;= Number(id.substring(9))){
								$(this).css("display", "none");

								$('#txtDate' + (idx + 1)).attr('value', prevSegDate);

								$('#txtDate' + (idx + 1)).data('initDate1', prevSegDate);
								$('#txtDate' + (idx + 1)).data('initDate', prevSegDate.replace(/[^0-9]/gi,''));
								$('#txtDate' + (idx + 1)).data('selectDate', prevSegDate.replace(/[^0-9]/gi,''));
								$('#txtDate' + (idx + 1)).data('selectDate1', prevSegDate);
							}
						});

						if(jsMaxSection != jsSelectSectionSize){
							$("#divAddSection").css("display", "block");
						}
					}

				}



				jsRouteTypeArray[id.substring(9)] = routeType;
					}

			if (connectionFlag == "M") {
				jsDualFlag = true;
			} else {
				jsDualFlag = false;
			}

			$("#txt"+id.substring(3)).val(airName);
			$("#hid"+id.substring(3)).val(airCode);
			$('#hidRouteType').val(routeType);

			if (connectionFlag == "M") {
				$('#hidDualFlag').val("Y");
			} else {
				$('#hidDualFlag').val("");
			}

			if ( $('#hidRouteType').val() == "D" ){
				jsRouteType = "DOM";
				jsRouteType2 = $("#hidRouteType").val();
			} else if ( $('#hidRouteType').val() == "I" ){
				jsRouteType = "INT";
				jsRouteType2 = $("#hidRouteType").val();
			}

			// 안내 팝업을 띄우기 위해 현재 Layer를 Close
			deleteLayer();

			// 출/도착지 초기화
			if( id.indexOf("Dep") != -1 ){

				//마지막 구간의 도착지체크값 초기화
				$("#hidLastArrStnCity").val("");

				if ( $("#hidTripType").val() != "M" ){
					// 왕복 &amp; 편도
					if ( $("#txtArrStn2").val() != "" &amp;&amp; $("#hidArrStn2").val() != "" ) {
						$("#txtArrStn2").val($.cfn_setText(jsonData, "lblArr"));
						$("#hidArrStn2").val("");
					}

					if (id.substring(9) == "1" ) {
						$('#hidCountry2').val(country);
					}

				} else {
					// 다구간

					if(id.indexOf("Dep") != -1){
						for (var i = Number(id.substring(9)); i &lt;= jsMaxSection; i++) {

							var nextIndex = i+1;

							if ( $("#txtArrStn" + i).val() != "" &amp;&amp; $("#hidArrStn" + i).val() != "" ) {
								$("#txtArrStn" + i).val($.cfn_setText(jsonData, "lblArr"));
								$("#hidArrStn" + i).val("");
							}

							if ( $("#txtDepStn" + nextIndex).val() != "" &amp;&amp; $("#hidDepStn" + nextIndex).val() != "" ){
								$("#txtDepStn" + nextIndex).val($.cfn_setText(jsonData, "lblDep"));
								$("#hidDepStn" + nextIndex).val("");
							}

							if ( $("#txtArrStn" + nextIndex).val() != "" &amp;&amp; $("#hidArrStn2").val() != "" ) {
								$("#txtArrStn" + nextIndex).val($.cfn_setText(jsonData, "lblArr"));
								$("#hidArrStn" + nextIndex).val("");
							}

							jsRouteTypeArray[i] = "";

						}
					}

					$.each( $(jsRouteTypeArray), function(idx){
						if(jsRouteTypeArray[idx] == "I") {
							routeType = "I"; //하나라도 국제선이면 국제선운행
							return false;
						} else {
							routeType = "D";
						}
					});

					$('#hidRouteType').val(routeType);


					if(id.substring(9) == "1") {
						jsRouteType2 = "";
					} else {
						jsRouteType2 = $('#hidRouteType').val();
					}

					// 다구간 현재 출발지의 국가코드를 세팅
					// 다음 출발지를 조회하기 위함
					var nextIndex = Number(id.substring(9))+1;

					$('#hidCountry' + id.substring(9)).val(country);
					jsCountryArray[id.substring(9)] = country;
				}
			}

			if(id.indexOf("Arr") != -1){

				if(routeType == "I"){

					if ($("#hidTripType").val() == "M" &amp;&amp; id.substring(9) == "1" ) {
						// 다구간 구간1 도착지의 국가코드를 세팅
						// 구간2 출발지를 조회하기 위함
						$('#hidCountry3').val(country);
						jsCountry2 = country;
					} else if ($("#hidTripType").val() == "M" &amp;&amp; id.substring(9) == "2" ){
						$('#hidCountry4').val(country);
					} else if ($("#hidTripType").val() != "M" &amp;&amp; id.substring(9) == "2" ){
						$('#hidCountry1').val(country);
					}

				}

				if ( $("#hidTripType").val() == "M" ) {
					//직전구간의 국가코드에 따라 다음 출발지가 결정된다.
					jsRouteType2 = jsRouteTypeArray[id.substring(9)];
				}
			}

			// 다음 출/도착지를 미리 조회
			if ( $("#hidTripType").val() != "M" ) {
				if( id.indexOf("DepStn1") != -1 ){
					jsStnId = "txtArrStn2";				// 구간2 도착지를 조회하기 위함
					jsDepStn = airCode;					// 선택한 출발지 공항코드
					jsRouteType2 = "";					// 국내/국제 구분자 초기화
					$.cfn_airportSearchTerms();			// 출/도착지 조회조건 세팅
				}
			} else {
				//첫구간 출발지의 국가가 현재 선택한 도착구간의 국가가 됐을때 이후의 구간을 hide시킨다.
				if(id == 'divDepStn1'){
					$('#hidFirstDepStnCity').val(cityCode);
					$('#hidFirstDepStnAirCode').val(airCode);
				}
				if(id.indexOf("divArr") != -1) $('#hidLastArrStnCity').val(cityCode);

				var nextIndex = Number(id.substring(9))+1;

				if (id.substring(9) == 1 &amp;&amp; id.indexOf("DepStn") != -1) {
					jsNationCode = "";
				} else {
					jsNationCode = jsCountryArray[1];					// 출발지의 국가코드
				}

				if( id.indexOf("DepStn") != -1 ){
					jsStnId = "txtArrStn" + id.substring(9);	// 다음구간 도착지를 조회하기 위함
					jsDepStn = airCode;					// 선택한 출발지 공항코드

					$.cfn_airportSearchTerms();			// 출/도착지 조회조건 세팅
				}

				if( id.indexOf("ArrStn") != -1 ){
					jsStnId = "txtDepStn" + nextIndex;	// 다음구간 출발지를 조회하기 위함
					//마지막구간 도착지 선택시는 이후 선택지를 셋팅할 필요없음.
					if(id.substring(9) != jsMaxSection){
						$.cfn_airportSearchTerms();			// 출/도착지 조회조건 세팅
					}
				}
			}

			// 구간에 따라 스케줄 제어
			if ( $("#hidTripType").val() == "R" ) {
				jsDateObj = $.cfn_airportChange($("#hidDepStn1").val(), $("#hidArrStn2").val(), "txtDate1", "txtDate2", $("#hidTripType").val(), jsLang);

				if ( jsDateObj ){
					var depMinDate = $.cfn_getDateFormat(jsDateObj.depMinDate);				// yyyy-MM-dd
					var retMinDate = $.cfn_getDateFormat(jsDateObj.retMinDate);				// yyyy-MM-dd
					var depWeekController = jsDateObj.weekController1;
					var retWeekController = jsDateObj.weekController2;
					var depMaxDate = jsDateObj.depMaxDate;
					var retMaxDate = jsDateObj.retMaxDate;

					if ( depWeekController != "" ) {
						$("#txtDate1").data("weekArea", depWeekController);
					} else {
						$("#txtDate1").data("weekArea", "");
					}

					if ( retWeekController != "" ) {
						$("#txtDate2").data("weekArea", retWeekController);
					} else {
						$("#txtDate2").data("weekArea", "");
					}

					if ( typeof(jsDateObj.depSelectDate) != "undefined" ) {
						var selectDepDate = $.cfn_getDateFormat(jsDateObj.depSelectDate);		// yyyy-MM-dd

						// 변경된 일자로 변경
						$("#txtDate1").val(selectDepDate);							// yyyy-MM-dd
						$("#hidDepDate").val(selectDepDate); 								// yyyy-MM-dd

						// 변경된 일자 달력 클릭 시 세팅
						$("#txtDate1").data("initDate", jsDateObj.depSelectDate);	// yyyyMMdd
						$("#txtDate1").data("initDate1", selectDepDate);			// yyyy-MM-dd
						$("#txtDate1").data("selectDate", jsDateObj.depSelectDate);	// yyyyMMdd
						$("#txtDate1").data("selectDate1", selectDepDate);			// yyyy-MM-dd
					}

					if ( typeof(jsDateObj.retSelectDate) != "undefined" ) {
						var selectRetDate = $.cfn_getDateFormat(jsDateObj.retSelectDate);		// yyyy-MM-dd

						// 변경된 일자로 변경
						$("#txtDate2").val(selectRetDate);								// yyyy-MM-dd
						$("#hidRetDate").val(selectRetDate); 								// yyyy-MM-dd

						// 변경된 일자 달력 클릭 시 세팅
						$("#txtDate2").data("initDate", jsDateObj.retSelectDate);		// yyyyMMdd
						$("#txtDate2").data("initDate1", selectRetDate);				// yyyy-MM-dd
						$("#txtDate2").data("selectDate", jsDateObj.retSelectDate);	// yyyyMMdd
						$("#txtDate2").data("selectDate1", selectRetDate);				// yyyy-MM-dd
					}

					$("#txtDate1").data("restDate", jsDateObj.depMinDate);			// yyyyMMdd
					$("#txtDate1").data("restDate1", depMinDate);					// yyyy-MM-dd
					$("#txtDate1").data("maxDate1", depMaxDate);
					$("#txtDate2").data("restDate", jsDateObj.retMinDate);				// yyyyMMdd
					$("#txtDate2").data("restDate1", retMinDate);						// yyyy-MM-dd
					$("#txtDate2").data("maxDate1", retMaxDate);
				} else {
					if ( $("#hidDepStn1").val() != "" &amp;&amp; $("#hidArrStn2").val() != "" ) {
						// 최초 운항일자가 있을 경우
						if ( $("#txtDate1").data("restDate") != "" ) {
							// 현재 선택한 날짜가 최초 운항일자보다 작을 경우
							$("#txtDate1").data("restDate", "");
							$("#txtDate1").data("restDate1", "");
							$("#txtDate1").data("weekArea", "");
						}

						if ( $("#txtDate2").data("restDate") != "" ) {
							// 현재 선택한 날짜가 최초 운항일자보다 작을 경우
							$("#txtDate2").data("restDate", "");
							$("#txtDate2").data("restDate1", "");
							$("#txtDate2").data("weekArea", "");
						}
					}
				}
			} else if ( $("#hidTripType").val() == "O" ){
				jsDateObj = $.cfn_airportChange($("#hidDepStn1").val(), $("#hidArrStn2").val(), "txtDate1", "", $("#hidTripType").val(), jsLang);

				if ( jsDateObj ){
					var depMinDate = $.cfn_getDateFormat(jsDateObj.depMinDate);				// yyyy-MM-dd
					var depWeekController = jsDateObj.weekController1;
					var depMaxDate = jsDateObj.depMaxDate;

					if ( depWeekController != "" ) {
						$("#txtDate1").data("weekArea", depWeekController);
					} else {
						$("#txtDate1").data("weekArea", "");
					}

					if ( typeof(jsDateObj.depSelectDate) != "undefined" ) {
						var selectDepDate = $.cfn_getDateFormat(jsDateObj.depSelectDate);		// yyyy-MM-dd

						// 변경된 일자로 변경
						$("#txtDate1").val(selectDepDate);							// yyyy-MM-dd
						$("#hidDepDate").val(selectDepDate); 								// yyyy-MM-dd

						// 변경된 일자 달력 클릭 시 세팅
						$("#txtDate1").data("initDate", jsDateObj.depSelectDate);	// yyyyMMdd
						$("#txtDate1").data("initDate1", selectDepDate);			// yyyy-MM-dd
						$("#txtDate1").data("selectDate", jsDateObj.depSelectDate);	// yyyyMMdd
						$("#txtDate1").data("selectDate1", selectDepDate);			// yyyy-MM-dd
					}

					$("#txtDate1").data("restDate", jsDateObj.depMinDate);			// yyyyMMdd
					$("#txtDate1").data("restDate1", depMinDate);					// yyyy-MM-dd
					$("#txtDate1").data("maxDate1", depMaxDate);
				} else {
					if ( $("#hidDepStn1").val() != "" &amp;&amp; $("#hidArrStn2").val() != "" ) {
						// 최초 운항일자가 있을 경우
						if ( $("#txtDate1").data("restDate") != "" ) {
							// 현재 선택한 날짜가 최초 운항일자보다 작을 경우
							$("#txtDate1").data("restDate", "");
							$("#txtDate1").data("restDate1", "");
							$("#txtDate1").data("weekArea", "");
						}
					}
				}
			}


			jsAddFlag = addFlag;
			jsDescDetail = descDetail;

			// addFlag가 "Y"이면 가져온 메시지 alert 2014.06.03
			if ( jsAddFlag == "Y" ){

				if ( descDetail == "undefined" ){
					// descDetail 값이 없을 경우
					jsDescDetail = "";
				} else if ( descDetail != "" ) {
					jsDescDetail = descDetail;
				} else {
					// descDetail 값이 없을 경우
					jsDescDetail = "";
				}
				alert(jsDescDetail);
			}
			//[인터넷부킹고도화]
			if ( id.indexOf("DepStn") != -1 ) {
				var destinationStn = "";
				destinationStn = id.replace("Dep", "Arr");
				if( $("#hidTripType").val() == "M"){
					$("#"+destinationStn).trigger("click");
				}else{
					$("#"+destinationStn.substring(0,destinationStn.length-1)+"2").trigger("click");
				}
			}else if ( id.indexOf("ArrStn") != -1 ) {
				if( $("#hidTripType").val()  == "M"){
					var nextSectionNo = id.substring(id.length-1,id.length);
					if($("#dateCal"+nextSectionNo).is(':visible')){
						$("#dateCal"+nextSectionNo).trigger("click");
					}
				}else{
					$("#dateCal1").trigger("click");
				}
			}
		}	// fn_setAirport()

		/////////////////////////////////////////////////////////////////////////
		// C9 에서 작업한 스크립트 시작
		/////////////////////////////////////////////////////////////////////////
		   function noticeDetail(bbs_id ,bbs_type) {
		    $("#bbs_id").val(bbs_id);
		    $("#bbs_type").val(bbs_type);
			//var sHost = jsMainReturnHost +"/"+ jsContextRoot;
		    $("#frmNotiValue").attr("action", "/jejuair/kr/com/jeju/ibe/news/notice/notice_detail.do").submit();
	    }


		function imgRolling(mom,img,elem,txt,delay,navi,mod){
			// 메인 비주얼영역
			/*
			var imgWidth = 1130; // 이미지 넓이
			var obj = jQuery(".imgList li");
			var txtObj = jQuery(".visualTxt li");
			var currentNum = 0;
			var isClick = false;
			var speed = 4000; //자동롤링 속도
			*/
			var parentObj = mom,
				imgWidth = img, // 이미지 넓이
				obj = elem,
				txtObj = txt,
				currentNum = 0,
				isClick = false,
				speed = delay, //자동롤링 속도
				rollingNavi = navi,
				mode = mod;

			jQuery(parentObj+" .nextBtn").on("click",function(){
				jQuery(parentObj+" .playBt").show();
				jQuery(parentObj+" .stopBt").hide();
				clearInterval(timer);
				nextPlay();
			});
			jQuery(parentObj+" .prevBtn").on("click",function(){
				jQuery(parentObj+" .playBt").show();
				jQuery(parentObj+" .stopBt").hide();
				clearInterval(timer);
				prevPlay()
			});

			function nextPlay(){
				if(mode == "fade"){
					obj.parent().addClass("fade");
					obj.not(obj.eq(currentNum)).css("opacity",0);
					if(isClick == false){
						isClick = true;
						currentNum++;

						if(currentNum &gt; obj.size()-1) currentNum=0;
						rollingNavi.removeClass("active");
						rollingNavi.eq(currentNum).addClass("active");
						obj.eq(currentNum-1).animate({"opacity":0},1000,"easeInOutQuint",function(){
							obj.css("z-index",80)
							obj.eq(currentNum).css("z-index",90)
							isClick = false;
						});
						obj.eq(currentNum).animate({"opacity":1},1000,"easeInOutQuint");
						if(txtObj != ""){
							txtObj.css("z-index",99);
							txtObj.eq(currentNum).css("z-index",100);
							txtObj.eq(currentNum-1).animate({"opacity":0},500,"easeInOutQuint");
							txtObj.eq(currentNum).animate({"opacity":1},500,"easeInOutQuint");
						}
					};
				}else if(mode == "slide"){
					obj.parent().removeClass("fade").addClass("slide");
					obj.not(obj.eq(currentNum)).css("left",imgWidth);
					if(isClick == false){
						isClick = true;
						currentNum++;

						if(currentNum &gt; obj.size()-1) currentNum=0;
						rollingNavi.removeClass("active");
						rollingNavi.eq(currentNum).addClass("active");
						obj.eq(currentNum-1).animate({"left":-imgWidth},1000,"easeInOutQuint",function(){
							obj.css("left",imgWidth);
							isClick = false;
						});
						obj.eq(currentNum).animate({"left":0},1000,"easeInOutQuint");
						if(txtObj != ""){
							txtObj.css("z-index",99);
							txtObj.eq(currentNum).css("z-index",100);
							txtObj.eq(currentNum-1).animate({"opacity":0},500,"easeInOutQuint");
							txtObj.eq(currentNum).delay(400).animate({"opacity":1},500,"easeInOutQuint");
						}
					};
				};
			};

			function prevPlay(){
				if(mode == "fade"){
					obj.parent().addClass("fade");
					obj.not(obj.eq(currentNum)).css("opacity",0);
					if(isClick == false){
						isClick = true;
						obj.eq(currentNum).animate({"opacity":0},1000,"easeInOutQuint");
						if(txtObj != ""){
							txtObj.eq(currentNum).animate({"opacity":0},500,"easeInOutQuint");
						};
						currentNum--;
						if(currentNum &lt; 0) currentNum=(obj.size()-1);
						rollingNavi.removeClass("active");
						rollingNavi.eq(currentNum).addClass("active");
						obj.eq(currentNum).animate({"opacity":1},1000,"easeInOutQuint",function(){
							obj.css("z-index",80)
							obj.eq(currentNum).css("z-index",90)
							isClick = false;
						});
						if(txtObj != ""){
							txtObj.css("z-index",99);
							txtObj.eq(currentNum).css("z-index",100);
							txtObj.eq(currentNum).animate({"opacity":1},500,"easeInOutQuint");
						};
					};
				}else if(mode == "slide"){
					obj.not(obj.eq(currentNum)).css("left",-imgWidth);
					if(isClick == false){
						isClick = true;
						obj.eq(currentNum).animate({"left":imgWidth},1000,"easeInOutQuint");
						if(txtObj != ""){
							txtObj.eq(currentNum).animate({"opacity":0},500,"easeInOutQuint");
						};
						currentNum--;
						if(currentNum &lt; 0) currentNum=(obj.size()-1);
						rollingNavi.removeClass("active");
						rollingNavi.eq(currentNum).addClass("active");
						obj.eq(currentNum).animate({"left":0},1000,"easeInOutQuint",function(){
							obj.not(obj.eq(currentNum)).css("left",-imgWidth);
							isClick = false;
						});
						if(txtObj != ""){
							txtObj.css("z-index",99);
							txtObj.eq(currentNum).css("z-index",100);
							txtObj.eq(currentNum).delay(400).animate({"opacity":1},500,"easeInOutQuint");
						};
					};
				};
			};

			jQuery(parentObj+" .stopBt").on("click",function(){ // 자동롤링 중지
				jQuery(parentObj+" .playBt").show().focus(); /* 웹접근성 개선수정 20141030 */
				jQuery(parentObj+" .stopBt").hide();
				while(timer){ /* 웹접근성 개선수정 20141030 :: START */
					clearInterval(timer);
					timer = null;
				}; /* 웹접근성 개선수정 20141030 :: END */
				clearInterval(timer);
				oldPlayUse = "stop"; /* 웹접근성 개선수정 20141030 */
				return false;
			});

			jQuery(parentObj+" .playBt").on("click",function(){
				jQuery(parentObj+" .stopBt").show().focus(); /* 웹접근성 개선수정 20141030 */
				jQuery(parentObj+" .playBt").hide();
				oldPlayUse = "play"; /* 웹접근성 개선수정 20141030 */
				imgPlay();
				return false;
			});

		/* 웹접근성 개선수정 20141030
		jQuery(obj).on("focusin",function(){
			if(mode == "fade"){
				jQuery(obj).stop().animate({"opacity":0},1000,"easeInOutQuint");
				jQuery(this).stop().animate({"opacity":1},1000,"easeInOutQuint",function(){
					jQuery(obj).css("z-index",90)
				});
			}
			jQuery(parentObj+" .playBt").show();
			jQuery(parentObj+" .stopBt").hide();
			clearInterval(timer);
			return false;
		})

		jQuery(obj).on("focusout",function(){
			if(mode == "fade"){
				var last = jQuery(obj).size()-1
					now = jQuery(this).index()
					console.log("last"+last+","+now);
				if(last == now){
					jQuery(this).stop().animate({"opacity":1},1000,"easeInOutQuint",function(){
						jQuery(this).css("z-index",90)
					});
				}else{
					jQuery(obj).stop().animate({"opacity":0},1000,"easeInOutQuint",function(){
					jQuery(obj).css("z-index",80)
				});
				}

			}
			jQuery(parentObj+" .stopBt").show();
			jQuery(parentObj+" .playBt").hide();
			imgPlay();
			return false;
		})
		*/

			/*
			var timer = setInterval(function(){ // 처음 시작 시 자동 롤링
				nextPlay();
			},speed);
			*/
			/* 웹접근성 개선수정 20141030 :: START */
			var timer = null;
			var imgPlayIdx = 0;

			function imgPlay(){
				while(timer){
					clearInterval(timer);
					timer = null;
				};
				timer = setInterval(function(){
					nextPlay();
				},speed);
			};
			imgPlay();
			/* 웹접근성 개선수정 20141030 :: END */



			/* 웹접근성 개선수정 20141030 :: START */
			var oldPlayUse = "play"; // 플레이 중이였는지, 아닌지 판단.
			$(mom).on("focusin",function(){

				// 메인 비주얼 용 접근성 컨트롤 추가.
				$(mom).find(".visualTitle a").each(function(i){
					this.idx = i;
					$(this).on("focusin",function(){
						while(timer){
							clearInterval(timer);
							timer = null;
							jQuery(parentObj+" .playBt").show();
							jQuery(parentObj+" .stopBt").hide();
						};

						currentNum = this.idx;
						rollingNavi.removeClass("active");
						rollingNavi.eq(currentNum).addClass("active");

						if(mode == "fade"){

							obj.css({"opacity":0,"z-index":80});
							obj.eq(currentNum).css({"opacity":1,"z-index":90});


						}else if(mode == "slide"){

							obj.css({"left":-imgWidth,"opacity":1});
							obj.eq(currentNum).css({"left":0,"opacity":1});

						}

						if(txtObj != ""){
							txtObj.css({"z-index":99,"opacity":0});
							txtObj.eq(currentNum).css({"z-index":100,"opacity":1});
						}



					});

				});


			});

			//[인터넷부킹고도화]
			$(".nextBtn, .prevBtn").on("focusout",function(){
				if(oldPlayUse=="play"){
					jQuery(parentObj+" .playBt").hide();
					jQuery(parentObj+" .stopBt").show();
				}
			});

			$(mom).on("focusout",function(){
				while(timer){
					clearInterval(timer);
					timer = null;
				};
				if(oldPlayUse=="play"){
					imgPlay();
				}
			});

			/* 웹접근성 개선수정 20141030 :: END */


			/////////////////////////////////////////////////////////////////////////
			// 단체예약 신청
			/////////////////////////////////////////////////////////////////////////
			// 내국인 전용 메뉴
			if(jsLang=="KR"){
				$("#application_form").show();
				$("#application_form").text("단체예약 신청");

			}else{
				$("#application_form").hide();
				$("#application_form").text("");
			}

			//단체예약신청 화면 이동
			$('#application_form').click(function(){
				location.href = "https://www.jejuair.net/jejuair/kr/com/jeju/ibe/booking/group/application.do";

			});

		};
		/////////////////////////////////////////////////////////////////////////
		// C9 에서 작업한 스크립트 끝
		/////////////////////////////////////////////////////////////////////////
	</script>


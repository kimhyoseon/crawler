<script type="text/javascript">
var jsProtocol   = "";
var jsReturnHost = "";
var loginChk = "false";
if (window.location.hostname == "www.jejuair.net" || window.location.hostname == "t4.jejuair.net" || window.location.hostname == "t2.jejuair.net" || window.location.hostname == "t3.jejuair.net"
	|| window.location.hostname == "twww.jejuair.net" || window.location.hostname == "dwww.jejuair.net") {
	jsProtocol = "https://";
  } else {
	  jsProtocol = "http://";
 }

if ( "localhost" == window.location.hostname ) {
	jsReturnHost = jsProtocol + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
} else {
	var port = "";
	if (window.location.hostname != "www.jejuair.net") {
		if(window.location.hostname == "dwww.jejuair.net") {
			port = ":490";
		} else if (window.location.hostname == "t1.jejuair.net") {
			port = ":443";
		}
	}
	jsReturnHost = jsProtocol + window.location.hostname + port;
}

//다국어 SEO 처리를 위한 언어코드 분리(20170420)
var sellang= "";
if(window.location.href.indexOf("/jejuair/") &gt; 0 ){
	var tmplang = window.location.href.substr(window.location.href.indexOf("/jejuair/")+8,4);
	if(tmplang == '/kr/' || tmplang == '/jp/' || tmplang == '/en/' || tmplang == '/cn/' || tmplang == '/th/'
		|| tmplang == '/hk/' || tmplang == '/vn/' || tmplang == '/ho/' || tmplang == '/tw/' || tmplang == '/mf/' || tmplang == '/my/' || tmplang == '/ph/' || tmplang == '/ru/' || tmplang == '/la/'){
		sellang = tmplang.charAt(1).toLowerCase() + tmplang.charAt(2).toLowerCase();
	}
}


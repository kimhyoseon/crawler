			$("input#ck1, input#ck2, input#ck3").on("click",function(){
				var ckVal  = jQuery(this).attr("value");
				var onVal  = "";
				var offVal = "";

				if(ckVal == "0") {
					onVal  = "0";
					offVal = "1";
				}
				else if(ckVal == "1") {
					onVal  = "1";
					offVal = "0";
				}

				if(ckVal == "Y") {
					onVal  = "Y";
					offVal = "N";
				}
				else if(ckVal == "N") {
					onVal  = "N";
					offVal = "Y";
				}

				if(jQuery(this).hasClass("ck")){
					jQuery(this).removeClass("ck");
					jQuery(this).siblings("input:hidden").val(offVal);
				}else{
					jQuery(this).addClass("ck");
					jQuery(this).siblings("input:hidden").val(onVal);
				}
			});


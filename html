	<script>
		$(document).ready(function($){
			//포인트 영역 마우스오버
			$('.mTab.mPoint .btnOpen').on('mouseenter',function(e){
				e.preventDefault();
				$('.mTab.mPoint').addClass('open');
				$('.mTab.mPoint').stop().animate({
					'width' : 440
				},450,'swing');
				$('.mTab.mPoint .ablePoint dl').animate({
					'width' : 140,
					'right': 10
				},100,'swing');
			});
			//포인트 영역 마우스아웃
			$('.mTab.mPoint').on('mouseleave',function(e){
				e.preventDefault();
				$('.mTab.mPoint').removeClass('open');
				$('.mTab.mPoint').stop().animate({
					'width' : 140
				},450,'swing');
				$('.mTab.mPoint .ablePoint dl').delay(300).animate({
					'width' : 125,
					'right': 0
				},100,'swing');
			});

			//예매하기 기본값 '왕복'으로 셋팅(20170607)
			$("#btnTripReturn").prop("checked", true);
		});
	</script>

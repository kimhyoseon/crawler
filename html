					$("#divPaxBox").removeClass("tr");
					$("#divSelADT").removeClass("w85");
					$("#divSelCHD").removeClass("w85");
					$("#divSelINF").removeClass("w85");
					$("#divBtnAgeCalc").removeClass("w85");

					//20160513 예매 영역 리뉴얼
					//$("#divSelADT").addClass("w130");
					//$("#divSelCHD").addClass("w130");
					//$("#divSelINF").addClass("w130");
					//$("#divBtnAgeCalc").addClass("w130");
					//$("#divBtnReservation").addClass("mr10");

					jsMultiFlag = "Y";
					jsRouteType2 = "";
					jsStnId = "txtDepStn1";		// 다구간 구간1 출발지를 조회하기 위함

					jsSelectSectionSize = 2;

					$('#depDateCal').attr("title",$.cfn_setText(jsonData,"depDateCal1")); //구간1 출발일 날짜 선택 달력
					$('#retDateCal').attr("title",$.cfn_setText(jsonData,"depDateCal2")); //구간2 출발일 날짜 선택 달력

					// 출발지 조회조건 세팅
					$.cfn_airportSearchTerms();

				}

				// 조회조건 초기화
				fn_setCalendar();		// 달력세팅
				fn_selSectionInit();	// 구간1, 구간2 초기화
				fn_personsInit(); 		// 인원 초기화
